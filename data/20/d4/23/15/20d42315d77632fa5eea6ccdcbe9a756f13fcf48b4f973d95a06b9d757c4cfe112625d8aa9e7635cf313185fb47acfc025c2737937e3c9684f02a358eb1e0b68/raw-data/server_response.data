<div style="TEXT-ALIGN:left"><font face="courier new,monospace" size="2">        my $html_body = undef;</font></div>
</li><li>
<div style="TEXT-ALIGN:left"><font face="courier new,monospace" size="2">        my $ua = LWP::UserAgent-&gt;new;<br />        $ua-&gt;timeout($timeout);<br />        my %form = ('DialogText' =&gt; 'to+reset+to+default', 'Advanced' =&gt; 16);<br />        my $response = $ua-&gt;post('http://' . $ip . '/process_adv/', \%form);<br />        if($response-&gt;is_success) {<br />            $html_body = $response-&gt;content;<br />            print $html_body;<br />        } else {<br />            print "Connect to $ip failed: " . $response-&gt;status_line . "\n";<br />        }</font></div>
</li><li>
<div style="TEXT-ALIGN:left"><font face="courier new,monospace" size="2">        return $html_body;<br />    }</font></div>
