		// blog lazy load
		$( document ).ready(function() {
			setTimeout(function() {
			     $('.infinite-container').addClass("loaded");
			}, 500);
			if ($(".infinite-container:last-of-type").find(".infinite-more-link").length == 0) {
				$('.more-wrap').hide();
			}
		});

		$(".more-link").on('click', function(event) {
			event.preventDefault(); // prevent the browser from navigating to this page
			var url = $(".infinite-container:last-of-type").find(".infinite-more-link").last().attr('href');
			if ($(".infinite-container:last-of-type").find(".infinite-more-link").length > 0) {
			    // it exists
				$.get(url, function(data)
		        {
		            $(data).find(".infinite-container").appendTo("#infinite-wrap");
		        })
		        .done(function() {
				    setTimeout(function() {
					     $('.infinite-container').addClass("loaded");
					     $('.infinite-container img').addClass("loaded");
					}, 500);
					if ($(".infinite-container:last-of-type").find(".infinite-more-link").length == 0) {
						$('.more-wrap').hide();
					}
				 });
			}
		});
