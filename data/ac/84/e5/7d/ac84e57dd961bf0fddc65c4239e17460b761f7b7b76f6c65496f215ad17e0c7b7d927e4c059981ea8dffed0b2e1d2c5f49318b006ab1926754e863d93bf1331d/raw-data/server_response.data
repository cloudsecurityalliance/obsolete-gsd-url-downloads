#include &lt;stdio.h&gt;
#include &lt;stdint.h&gt;
#include &lt;limits.h&gt;

template&lt;typename T&gt;
inline T
RotateLeft(const T aValue, uint_fast8_t aShift)
{
  return (aValue &lt;&lt; aShift) | (aValue &gt;&gt; (sizeof(T) * CHAR_BIT - aShift));
}

int main() {
    uint32_t rand_ = 0;
    uint64_t iters = 0;
    do {
        rand_ ^= RotateLeft(rand_, 5) ^ RotateLeft(rand_, 24);
        rand_ += 0x37798849;
        iters++;
    } while (rand_ != 0);

    fprintf(stdout, &quot;\ntook %zu iters\n&quot;, iters);
    return 0;
}
                                                                                                                                                                                     
