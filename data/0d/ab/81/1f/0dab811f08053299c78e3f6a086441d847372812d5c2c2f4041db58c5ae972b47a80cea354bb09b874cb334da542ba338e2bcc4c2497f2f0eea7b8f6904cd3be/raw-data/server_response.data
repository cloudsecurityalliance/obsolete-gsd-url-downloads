    #2 0x1df6f11 in lprn_print_image ghostpdl/./contrib/lips4/gdevlprn.c:260
    #3 0x1dd8051 in lips_print_page_copies ghostpdl/./contrib/lips4/gdevl4r.c:609:12
    #4 0x13ef028 in gdev_prn_output_page_aux ghostpdl/./base/gdevprn.c:1133:27
