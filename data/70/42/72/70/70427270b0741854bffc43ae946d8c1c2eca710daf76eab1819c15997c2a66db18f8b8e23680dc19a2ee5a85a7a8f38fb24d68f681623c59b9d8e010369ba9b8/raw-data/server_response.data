    <script>//<![CDATA[

        //TODO refactor ( disable selector overload )
        const normalizeFooter = (e) => {
            const $sidebar = $('.ia-fixed-sidebar');
            if($sidebar.length < 1) return;
            const $footer = $('.layout-footer');
            let wScrollTop = $(window).scrollTop();
            const $headerWrapper = $('.content-header-wrapper');

            let bottom = wScrollTop - ($footer.offset().top  - $(window).height());
            let top =  wScrollTop - $headerWrapper.offset().top  - $headerWrapper.height();
                $sidebar.css('height', '100%');
            if(bottom >= 0 ){
                //Velocity bug. Can't edit this direct in property
                bottom = bottom + 'px';
                e.stopImmediatePropagation();
                    $sidebar.css('height', 'auto');
                    $sidebar.css('bottom', bottom);
            }

            if( top<= 0) {
                //14 offset value
                top = -top + 14 + 'px';
                    $sidebar.css('top', top);
            } else {
                    $sidebar.css('top', 0);
            }
        }

        $(document).on('scroll', (e) => {
            normalizeFooter(e);
        });

        $(document).on('resize', (e) => {
            document.dispatchEvent(new Event('scroll'));
        });

        let oldDocumentHeight = $(document).height();
        const observer = new MutationObserver((mutationsList, observer) => {
            mutationsList.forEach( (mutation) => {
                if(mutation.addedNodes.length || mutation.removedNodes.length) document.dispatchEvent(new Event('scroll'));
            });
        });

        observer.observe( document.querySelector('#main'),{subtree: true, childList: true, characterData: true});
        document.dispatchEvent(new Event('scroll'));
    
//]]>
</script>

