:root {
	--color-primary: #86c141;
	--color-secondary: #132428;
	--color-black: #132428;
	--color-white: #ffffff;
	--color-accent: #86c141;
	--color-muted: #ddd;
	--spacer: 5px;
}

.uppercase,
.vc_custom_heading {
	text-transform: uppercase;
}

.bold {
	font-weight: bold;
}

.main-content {
    padding-top: 40px;
}

.home .main-content {
    padding-top: 0;
}


table {
    border: none;
}

.table tbody tr {
    font-size: 18px;
}

table th {
    font-size: 28px;
    line-height: 36px;
    text-transform: uppercase;
    font-weight: normal;
}

.mobile-only .menu {
    padding-top: 40px;
}

.nectar-love {
  display: none!important;
} 
.meta-comment-count {
    display: none !important;
}
.home .blog-recent .col .post-header h3, 
.blog-recent .col .post-header h3 {
  font-size: 24px!important;
	line-height: 1.2;
}
/* Heading for all slides*/
body .swiper-slide .content h2 {
    color: #00aebc !important;
}
#footer-outer #footer-widgets .col ul li a ,
.widget_text{
    font-size: 16px !important;
}
#footer-outer #footer-widgets {
  padding: 0px 0px!important;
  color: #969696 !important;
	color: var( --color-black );
}

.lang-item img { margin-bottom: 0 !important; }
#header-secondary-outer { top: 31px; }
