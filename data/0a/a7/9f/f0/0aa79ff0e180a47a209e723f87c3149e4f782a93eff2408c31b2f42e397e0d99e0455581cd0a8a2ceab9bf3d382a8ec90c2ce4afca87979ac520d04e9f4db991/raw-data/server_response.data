        //Fix for CORE20160114-Time Zones should be selected from a pre-defined list
        //For detecting user's time zone
        var isTimeZoneCookie = false;
        var cookieName = 'ClientTimezone';

        if (document.cookie.length > 0) {
            var timeZoneCookie = document.cookie.indexOf(cookieName + "=");

            if (timeZoneCookie != -1) {
                timeZoneCookie = timeZoneCookie + cookieName.length + 1;

                var timeZoneCookieEnd = document.cookie.indexOf(";", timeZoneCookie);

                if (timeZoneCookieEnd == -1) {
                    timeZoneCookieEnd = document.cookie.length;
                }
                if (document.cookie.substring(timeZoneCookie, timeZoneCookieEnd).length > 0) {
                    isTimeZoneCookie = true;
                }
            }
        }

        if (!isTimeZoneCookie) {
            var currentDate = new Date().toString();
            var userTimeZone = currentDate.match(/\(([^)]+)\)/)[1];

            document.cookie = cookieName + "=" + userTimeZone;
        }
