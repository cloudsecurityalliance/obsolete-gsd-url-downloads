valueType":{"__typename":"VariantFlagBoolean","value":true}},{"__typename":"VariantFlag","name":"use_new_admin_topic_backend","valueType":{"__typename":"VariantFlagBoolean","value":true}}],"viewer":null,"meterPost({\"postId\":\"9155eec36a34\",\"postMeteringOptions\":{\"referrer\":\"\",\"sk\":null,\"source\":null}})":{"__ref":"MeteringInfo:{}"},"postResult({\"id\":\"9155eec36a34\"})":{"__ref":"Post:9155eec36a34"}},"MeteringInfo:{}":{"__typename":"MeteringInfo","postIds":[],"maxUnlockCount":3,"unlocksRemaining":0},"User:e522509ca8e0":{"id":"e522509ca8e0","__typename":"User","name":"Kyle Hanslovan","username":"kylehanslovan","newsletterV3":{"__ref":"NewsletterV3:f3d21967df7e"},"customStyleSheet":null,"isSuspended":false,"bio":"Ethical Hacker. Malware Connoisseur. CEO at @HuntressLabs.","imageId":"1*5p9fa3-8SsS1Q6SL86JtMw.jpeg","hasCompletedProfile":false,"isAuroraVisible":true,"mediumMemberAt":0,"socialStats":{"__typename":"SocialStats","followerCount":310,"followingCount":49,"collectionFollowingCount":2},"customDomainState":{"__typename":"CustomDomainState","live":{"__typename":"CustomDomain","domain":"kylehanslovan.medium.com","status":"ACTIVE","isSubdomain":true}},"hasSubdomain":true,"viewerEdge":{"__ref":"UserViewerEdge:userId:e522509ca8e0-viewerId:lo_c4a9168840c4"},"bookAuthor":null,"isPartnerProgramEnrolled":false,"viewerIsUser":false,"homepagePostsConnection({\"paging\":{\"limit\":1}})":{"__typename":"PostConnection","posts":[{"__ref":"Post:68353beb4fb9"}]},"postSubscribeMembershipUpsellShownAt":0,"allowNotes":true,"replyToEmailBannerShownCount":0,"twitterScreenName":"KyleHanslovan","followedCollections":3,"referredMembershipCustomHeadline":"","referredMembershipCustomBody":"","atsQualifiedAt":1612205692823},"ImageMetadata:1*LoN1BaUA0OYmfI4NwzmiYA.png":{"id":"1*LoN1BaUA0OYmfI4NwzmiYA.png","__typename":"ImageMetadata"},"CollectionViewerEdge:collectionId:76ed00a32c0c-viewerId:lo_c4a9168840c4":{"id":"collectionId:76ed00a32c0c-viewerId:lo_c4a9168840c4","__typename":