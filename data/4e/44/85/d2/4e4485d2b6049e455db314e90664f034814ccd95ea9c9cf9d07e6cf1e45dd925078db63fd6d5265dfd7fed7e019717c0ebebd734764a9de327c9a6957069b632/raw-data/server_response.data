iv><div class='add'>+   if (!parse_identifier( &amp;ctx-&gt;cur, id, sizeof(id) )) {</div><div class='ctx'>       report_error( ctx, "Syntax error" );</div><div class='ctx'>       return FALSE;</div><div class='ctx'>    }</div></td></tr></table></div> <!-- class=content -->
