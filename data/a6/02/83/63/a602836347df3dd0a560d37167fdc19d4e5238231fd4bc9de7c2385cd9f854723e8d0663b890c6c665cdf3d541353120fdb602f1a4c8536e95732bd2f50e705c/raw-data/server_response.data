/**
 * pocketName is the unique prefix you give to the elements that
 *  hold the content in different languages (e.g. AdPocket-de);
 * showEnglishDefault will, if you don't set it to false, show English
 *  language content if none exists for the current language.
 *              
 *
**/
showTranslatedContent = function (pocketName, showEnglishDefault = true) {
    let lang = gdn.meta.ui.localeKey ?? 'en';
    let elementExistsInThisLang = null;
    let testElement = null;
    console.log('Loading Locale Content', lang);
    if (lang != 'en') {
        let englishElementID = pocketName+'-en';
        let englishElement = document.getElementById(englishElementID);
        console.log('englishElement', {'englishElement':englishElement, 'englishElementID':englishElementID});
        if (typeof(englishElement) != 'undefined' && englishElement != null) {
            console.log('english element exists', document.getElementById(pocketName+'-en').childNodes[1]);
            testElement = document.getElementById(pocketName+'-en').childNodes[1].className;
            elementExistsInThisLang = document.getElementById(pocketName+'-'+lang).getElementsByClassName(testElement).length;
        }
        if (elementExistsInThisLang) {
            document.getElementById(pocketName+'-en').setAttribute('style', 'display:none');
            document.getElementById(pocketName+'-'+lang).setAttribute('style', 'display:block');
        } else {
            if (showEnglishDefault == false) {
                document.getElementById(pocketName+'-en').setAttribute('style', 'display:none');
            } 
        }
        console.log('Loading Locale Content', {'lang':lang, 'pocketName':pocketName+'-'+lang, 'testElement': testElement, 'elementExsistInThisLang': elementExistsInThisLang, 'showEnglishDefault':showEnglishDefault});
    }
};
