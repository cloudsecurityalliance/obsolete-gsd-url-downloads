function dispImgChar(page){

  var darea = document.getElementById("comment-emoji-list");
  darea.style.display = "none";
  var typenode = 1;
  for(i=0; i<darea.childNodes.length; i++){
    if(darea.childNodes[i].nodeType != typenode){continue}
    darea.childNodes[i].style.display = "none";
  }

  if(page==0){
    document.cmform['comment_text'].focus();
    document.getElementById("emojioptstart").selected = "selected";
    return
  }
  
  var seekpage = "comment-emoji-list_" + page;
  if(document.getElementById(seekpage)){
    darea.style.display = "";
    document.getElementById(seekpage).style.display = "";
    return;
  }

  var adddiv = document.createElement("div");
  adddiv.id = seekpage;
  darea.appendChild(adddiv);
  
  page--;
  for(i=0; i<arImgName[page].length; i++){
    var srcdir = "https://blogimg.goo.ne.jp/img_emoji/" + arImgName[page][i];
    var eimg = document.createElement("img");
    eimg.onclick = function(){setName(this.src)};
    eimg.onmouseover = function(){setSelect(this)}
    eimg.onmouseout = function(){setSelect(this)}
    eimg.style.margin = "3px";
    eimg.src = srcdir;
    adddiv.appendChild(eimg);
  }

  darea.style.display = "";
  adddiv.style.display = "";
}

function setName(outstr){
  outstr = outstr.replace(/^.+\//, "");
  outstr = outstr.replace(/\.[^.]+$/,"");
  outstr = "{/" + outstr + "/}";
  puttxt(outstr);
  var tartext = document.cmform['comment_text'].focus();
}

function setSelect(imgS){
  if(imgS.style.backgroundColor){
    imgS.style.backgroundColor = "";
  }else{
    imgS.style.backgroundColor = "#CCC";
  }
}

function puttxt(instext){
  var tartext = document.cmform['comment_text'];
  if(document.selection){
    tartext.focus();
    var rng = document.selection.createRange();
    if(rng.text.length == 0){rng.text = instext}else{rng.text += instext};
    return;
  }
  if(tartext.selectionStart){
    var point = tartext.selectionEnd;
    if(!point){tartext.value += instext; return};
    var srcval = tartext.value;
    tartext.value = srcval.substr(0,point) + instext + srcval.substring(point,srcval.length);
    return;
  }
  tartext.value += instext;
}
