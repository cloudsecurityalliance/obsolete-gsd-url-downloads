                                                <script type="text/javascript">
                                                                function getTimezone()
                                                                {
                                                                                var tzo = new Date().getTimezoneOffset();
                                                                                //returns timezone offset in minutes
                                                                                function pad(num, digits)
                                                                                {
                                                                                                num = String(num);
                                                                                                while (num.length < digits)
                                                                                                {
                                                                                                                num = "0" + num;
                                                                                                };
                                                                                                return num;
                                                                                }
 
                                                                                return "GMT " + (tzo > 0 ? "-" : "+") + pad(Math.floor(Math.abs(tzo) / 60), 2) + ":" + pad(tzo % 60, 2);
                                                                }
 
                                                                var imgDate = new Date();
                                                                var months = ['January', 'February', 'March', 'April', 'May', 'June', 'July', 'August', 'September', 'October', 'November', 'December'];
                                                                var dt = months[imgDate.getMonth()] + '/' + imgDate.getDate() + '/' + imgDate.getFullYear() + ' ' + imgDate.getHours() + ':' + imgDate.getMinutes() + ':' + imgDate.getSeconds() + ' TZ: ' + getTimezone();
                                                                document.write('<p class="msg_text" align="right">' + dt +  '</p>');
                                                </script>


