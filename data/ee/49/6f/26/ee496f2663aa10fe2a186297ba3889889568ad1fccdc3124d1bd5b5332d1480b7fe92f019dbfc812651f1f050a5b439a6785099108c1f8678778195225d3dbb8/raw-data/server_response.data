              <source srcset="/sites/default/files/styles/full_width/public/image/tor-logo-blog_31.png?itok=E0UoF54I 1x, /sites/default/files/styles/full_width_retina/public/image/tor-logo-blog_31.png?itok=_Cv2tUvh 2x" media="screen and (min-width: 0px)" type="image/png"/>
