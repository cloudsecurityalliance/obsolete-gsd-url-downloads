// Tab operation cover
var tab_nav = document.querySelectorAll('.tab-nav');
for (var i = 0; i < tab_nav.length; i++) {
  tab_nav[i].addEventListener('click', function (e) {

    // Hide not active tab
    var tabDetailsTarget = this.parentElement.nextElementSibling.querySelectorAll('.tab-content');
    for (var i = 0; i < tabDetailsTarget.length; i++) {
      tabDetailsTarget[i].classList.remove('show');
    }
    
    var tab_nav_value = e.target.dataset.tabId
    var tab_nav_trigger = __('#' + tab_nav_value);
    tab_nav_trigger.classList.add('show');

    // Add class when active tab
    var addClassTab = this.parentElement.querySelectorAll('.tab-nav');
    for (var i = 0; i < addClassTab.length; i++) {
      addClassTab[i].classList.remove('active');
    }
    var addClassTab2 = this.parentElement.nextElementSibling.querySelectorAll('.tab-nav');
    for (var i = 0; i < addClassTab2.length; i++) {
      addClassTab2[i].classList.remove('active');
    }
    e.target.classList.add('active');
    e.preventDefault();
  });
}

// Tab switching depending on the parameter
$(function(){
  const hash = location.hash;
  if (hash == '#1st') {
      $('[data-tab-id="tab-1st"]').addClass('active');
      $('[data-tab-id="tab-2nd"]').removeClass('active');
      $('#tab-1st').addClass('show');
      $('#tab-2nd').removeClass('show');
  }else if(hash == '#2nd') {
      $('[data-tab-id="tab-2nd"]').addClass('active');
      $('[data-tab-id="tab-1st"]').removeClass('active');
      $('#tab-2nd').addClass('show');
      $('#tab-1st').removeClass('show');
  }
});

