    <script>
         $(document).ready(function() {
               $.get( "/plugins/custom/infoblox/infoblox/download_links_json_endpoints" ).done(function( data )
        {
            function isExist(array,value)
            {
                var returnValue=false;
                        for(var i=0;i<array.length;i++)
                        {
                            if(array[i] == value)
                            {
                                returnValue=true;
                                break;
                            }
                        }
                return returnValue;
            }
    
            var userTags=new Array();
                    userTags.push("Security Alerts".toLowerCase());
               
                var dict={};
                for(var i=0;i<data.length;i++)
                {
                    var jsonObject=data[i];
                    for(var j=0;j<jsonObject.Tags.length;j++)
                    {
                        if(isExist(userTags,jsonObject.Tags[j].Name.toLowerCase()))
                        {
                            jsonObject.Count=jsonObject.Count == undefined ? 0 : jsonObject.Count+1;
                            dict[jsonObject.Link]=jsonObject;
                            
                            
                        }
