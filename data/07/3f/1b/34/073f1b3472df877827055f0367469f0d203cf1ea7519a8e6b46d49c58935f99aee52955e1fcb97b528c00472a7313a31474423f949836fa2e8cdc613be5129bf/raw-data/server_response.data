            .toggleClass('growl_open growl_closed')
            .one(
            'webkitTransitionEnd otransitionend oTransitionEnd msTransitionEnd transitionend',
            function(e) {
                remArts.removeAttr('style');
                localStorage.setItem(
                'growlState',
                $('.tagline-offer-text').is(':visible')
                );
            }
            );
        var growlState = localStorage.getItem('growlState');
        if (growlState === 'false') {
            $('i')
            .removeClass('fa-angle-right')
            .addClass('fa-angle-left');
        } else {
            $('i')
            .removeClass('fa-angle-left')
            .addClass('fa-angle-right');
        }
        }

        $('#growl_bug').click(toggleBug);

        var growlState = localStorage.getItem('growlState');

        var growlState = localStorage.getItem('growlState');
        if (growlState === 'false') {
        $('.growl-col--inner__tagline, .col__cta-container').attr(
            'style',
            'visibility:hidden;'
        );
        $('.growl-wrapper').addClass('notransition');
        toggleBug();
        $('i')
            .removeClass('fa-angle-left')
            .addClass('fa-angle-right');
        $('.growl-wrapper')[0].offsetHeight;
        $('.growl-wrapper').removeClass('notransition');

        $('.growl-col--inner__tagline, .col__cta-container').removeAttr('style');
        }

    </script>

        

