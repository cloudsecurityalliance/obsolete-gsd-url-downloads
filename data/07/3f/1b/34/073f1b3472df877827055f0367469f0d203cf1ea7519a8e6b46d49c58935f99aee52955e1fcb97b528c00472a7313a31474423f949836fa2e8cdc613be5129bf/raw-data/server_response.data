        function setLocalStorage(name,value){
            localStorage.setItem(name, value);
        }
        
        function getLocalStorage(name){
            return localStorage.getItem(name);
        }

        function removeLocalStorageItem(name){
            localStorage.removeItem(name);
        }

                                                                                
            $(document).ready(function() {


                    var maxfreetopics = 10

                    if(getLocalStorage('maxfreearticleslimit')){
                        var articleList = JSON.parse(getLocalStorage('maxfreearticleslimit'));
                    }

                    if (articleList) {

                        if (articleList.includes(window.location.pathname) == false) {
                            articleList.push(window.location.pathname);
                        }

                        setLocalStorage('maxfreearticleslimit', JSON.stringify(articleList));
                        
                    } else {

                        var currentArticle = window.location.pathname;
                        var allArticles = [currentArticle];
                        setLocalStorage('maxfreearticleslimit', JSON.stringify(allArticles));

                    }

                    if(getLocalStorage('maxfreearticleslimit')){
                        var articleList = JSON.parse(getLocalStorage('maxfreearticleslimit'));
                    }else{
                        var articleList = [];
                    }
    
                    
                    var remtopics = maxfreetopics - articleList.length;

                    $('.remain-article').text(remtopics);

                    var botttomBoardtext =
                    'Reading ' + articleList.length + ' out of ' + maxfreetopics + ' free topics';

                    if (articleList.length > maxfreetopics) {
                        $('.growl-wrapper').hide();
                        document.getElementById('blogarticleoverlay').style.display = 'block';
