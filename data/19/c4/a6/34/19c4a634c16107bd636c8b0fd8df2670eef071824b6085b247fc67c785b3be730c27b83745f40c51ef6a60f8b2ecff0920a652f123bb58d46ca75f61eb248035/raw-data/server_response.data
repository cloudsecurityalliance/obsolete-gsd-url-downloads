<script>
    window.getPerformanceRecord = function () {
        var timing = window.performance.timing;
        var now = new Date();
        var pageName;
        if (typeof logPageLabel !== "undefined") {
            pageName = logPageLabel + '-' + BW_DEVICE;
        } else {
            pageName = 'Unknown';
            if (typeof BW_DEVICE !== "undefined") {
                pageName += ('-' + BW_DEVICE);
            }
            console.log('logPageLabel is not defined.');
        }

        var recordData = {
            "userAgent": window.navigator.userAgent,
            "page": pageName,
            "title": document.title,
            "url": location.href,
            "hostname": location.hostname,
            "unloadEventStart": (timing.unloadEventStart - timing.navigationStart),
            "unloadEventEnd": (timing.unloadEventEnd - timing.navigationStart),
            "redirectStart": (timing.redirectStart - timing.navigationStart),
            "redirectEnd": (timing.redirectEnd - timing.navigationStart),
            "fetchStart": (timing.fetchStart - timing.navigationStart),
            "domainLookupStart": (timing.domainLookupStart - timing.navigationStart),
            "domainLookupEnd": (timing.domainLookupEnd - timing.navigationStart),
            "connectStart": (timing.connectStart - timing.navigationStart),
            "connectEnd": (timing.connectEnd - timing.navigationStart),
            "secureConnectionStart": (timing.secureConnectionStart - timing.navigationStart),
            "requestStart": (timing.requestStart - timing.navigationStart),
            "responseStart": (timing.responseStart - timing.navigationStart),
            "responseEnd": (timing.responseEnd - timing.navigationStart),
            "domLoading": (timing.domLoading - timing.navigationStart),
            "domInteractive": (timing.domInteractive - timing.navigationStart),
            "domContentLoadedEventStart": (timing.domContentLoadedEventStart - timing.navigationStart),
            "domContentLoadedEventEnd": (timing.domContentLoadedEventEnd - timing.navigationStart),
            "domComplete": (timing.domComplete - timing.navigationStart),
            "loadEventStart": (timing.loadEventStart - timing.navigationStart),
            "loadEventEnd": (timing.loadEventEnd - timing.navigationStart),
            "created": now.getFullYear() + "-" + (now.getMonth() + 1) + "-" + now.getDate() + " " + now.getHours() + ":" + now.getMinutes() + ":" + now.getSeconds()
        };
        // paint timing api
        try {
            var performanceEntries = window.performance.getEntriesByType('paint');
            for (var i = 0; i < performanceEntries.length; i++) {
                var entry = performanceEntries[i];
                if (entry.name === 'first-paint' || entry.name === 'first-contentful-paint') {
                    recordData[entry.name] = entry.startTime;
                }
            }
        } catch (e) {
            console.log(e);
        }

        return recordData;
    };

    window.printEndUserPerformance = function () {
        var recordData = getPerformanceRecord();
        console.log("page: " + recordData.page);
        console.log("unloadEventStart: " + recordData.unloadEventStart);
        console.log("unloadEventEnd: " + recordData.unloadEventEnd);
        console.log("redirectStart: " + recordData.redirectStart);
        console.log("redirectEnd:" + recordData.redirectEnd);
        console.log("fetchStart:" + recordData.fetchStart);
        console.log("domainLookupStart: " + recordData.domainLookupStart);
        console.log("domainLookupEnd: " + recordData.domainLookupEnd);
        console.log("connectStart: " + recordData.connectStart);
        console.log("connectEnd: " + recordData.connectEnd);
        console.log("secureConnectionStart: " + recordData.secureConnectionStart);
        console.log("requestStart:" + recordData.requestStart);
        console.log("responseStart: " + recordData.responseStart);
        console.log("responseEnd: " + recordData.responseEnd);
        console.log("domLoading: " + recordData.domLoading);
        console.log("domInteractive:" + recordData.domInteractive);
        console.log("domContentLoadedEventStart:" + recordData.domContentLoadedEventStart);
        console.log("domContentLoadedEventEnd: " + recordData.domContentLoadedEventEnd);
        console.log("domComplete: " + recordData.domComplete);
        console.log("loadEventStart: " + recordData.loadEventStart);
        console.log("loadEventEnd: " + recordData.loadEventEnd);
        if ("first-paint" in recordData) {
            console.log("first-paint: " + recordData["first-paint"]);
        }
        if ("first-contentful-paint" in recordData) {
            console.log("first-contentful-paint: " + recordData["first-contentful-paint"]);
        }
    };
</script>



