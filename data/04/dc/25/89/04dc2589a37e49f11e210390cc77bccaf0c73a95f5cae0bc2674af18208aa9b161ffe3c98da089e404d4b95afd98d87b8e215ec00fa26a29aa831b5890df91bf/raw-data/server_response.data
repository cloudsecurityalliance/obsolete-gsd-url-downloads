<div class="main ">
	<div class="blog-post blog-post-wrapper">
		<div class="container">
			<article id="post-533" class="section section-text">
	<div class="row">
				<div class="col-md-10 single-post-container col-md-offset-1" data-layout="full-width">
			<div class="flex-row as-control-top">
				<div class="col-sm-5 as-search-col">
					<div class="as-search">
						<form action="https://blog.appcensus.io/" method="get" class="search-form form-group"><input type="text" name="s" id="s" class="form-control" placeholder="Search Blog" value="" required><input type="submit" class="as-search-submit" value="Search"></form>					</div>
				</div>
				<div class="col-sm-3 col-sm-offset-4 as-contact-col">
					<div class="as-contact"><a class="btn btn-primary" href="https://appcensus.io" target="_blank">Contact us</a></div>
				</div>
