    var shareUrl = 'https://another.rocomotion.net/12604561773482.html';
    var description = 'P forumをお使いの方へ | Another Rocomotion';
    
    function setSnsShare(shareUrl, description) {
        setTwitterLink(".twitter-button", shareUrl,description);
        setFacebookLink(".facebook-button", shareUrl, description);
        setLineLink(".line-button", shareUrl, description);
        setHatebuLink(".hatena-bookmark-button", shareUrl, description);
        setPocketLink(".pocket-button", shareUrl, description);
    }

    function setTwitterLink(shareSelector, shareUrl, description) {
        $(shareSelector).attr("href", "https://twitter.com/share?url=" + shareUrl + "&text=" + encodeURIComponent(description));
        setShareEvent(shareSelector, 'Twitter', shareUrl);
    }

    function setFacebookLink(shareSelector, shareUrl, description) {
        $(shareSelector).attr("href", "https://www.facebook.com/sharer/sharer.php?u=" + shareUrl + "&t=" + encodeURIComponent(description));
        setShareEvent(shareSelector, 'Facebook', shareUrl);
    }

    function setLineLink(shareSelector, shareUrl, description) {
        $(shareSelector).attr("href", "https://timeline.line.me/social-plugin/share?url=" + shareUrl);
        setShareEvent(shareSelector, 'LINE', shareUrl);
    }

    function setHatebuLink(shareSelector, shareUrl, description) {
        $(shareSelector).attr("href", "https://b.hatena.ne.jp/add?mode=confirm&shareUrl=" + shareUrl + "&description=" + encodeURIComponent(description));
        setShareEvent(shareSelector, 'Hatena Bookmark', shareUrl);
    }

    function setPocketLink(shareSelector, shareUrl, description) {
        $(shareSelector).attr("href", "http://getpocket.com/edit?url=" + encodeURIComponent(shareUrl));
        setShareEvent(shareSelector, 'Pocket', shareUrl);
    }

    function setShareEvent(selector, snsName, shareUrl) {
        $(selector).on('click', function(e){
            var current = this;
            ga('send', 'social', snsName, 'share', shareUrl, {
                'nonInteraction': 1
            });

            window.open(current.href, '_blank', 'width=650, height=450, menubar=no, toolbar=no, scrollbars=yes');
            e.preventDefault();
        }); 
    }
    function getHatenaBookmarkCount(entryUrl, selcter) {
      entryUrl = 'https://b.hatena.ne.jp/entry.count?url=' + encodeURIComponent(entryUrl)
      $.ajax({
        url:entryUrl,
        dataType:'jsonp',
      }).then(
        function(result){ $(selcter).text(result || 0); },
        function(){ $(selcter).text('0'); }
      );
    }
    function getFacebookCount(entryUrl, selcter) {
      $.ajax({
        url:'/',
        type: 'POST',
        data: {'u' : entryUrl, 'type' : 'facebookcount' },
        timeout: 10000
      }).then(
          function(result){
            if(result.count) {
                $(selcter).text(result.count);
            } else {
                $(selcter).text('0');
            }
        },
      	function(){ $(selcter).text('0'); }
      );
    }
    function getPocketCount(entryUrl, selcter) {
      $.ajax({
        url:'/',
        type: 'POST',
        data: {'u' : entryUrl, 'type' : 'pocketcount' },
        timeout: 10000
      }).then(
          function(result){
            if(result.count) {
                $(selcter).text(result.count);
            } else {
                $(selcter).text('0');
            }
        },
      	function(){ $(selcter).text('0'); }
      );
    }
    function getTweetCount(entryUrl, selcter) {
      $.ajax({
        url:'/',
        type: 'POST',
        data: {'u' : entryUrl, 'type' : 'tweetcount' },
        timeout: 10000
      }).then(
          function(result){
            if(result.count) {
                $(selcter).text(result.count);
            } else {
                $(selcter).text('0');
            }
        },
      	function(){ $(selcter).text('0'); }
      );
    }
    $(function(){
      setSnsShare(shareUrl, description);
      getHatenaBookmarkCount(shareUrl, '.hatena-bookmark-count');
      getFacebookCount(shareUrl, '.facebook-count');
      getPocketCount(shareUrl, '.pocket-count');
      getTweetCount(shareUrl, '.tweet-count');
    });
