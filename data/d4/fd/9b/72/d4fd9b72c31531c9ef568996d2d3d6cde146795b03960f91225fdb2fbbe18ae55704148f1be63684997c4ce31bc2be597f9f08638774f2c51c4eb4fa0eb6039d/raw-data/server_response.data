 	offset = blk - group_first_block;
+	if (offset &lt; 0 || EXT4_B2C(sbi, offset) &gt;= sb-&gt;s_blocksize ||
+	    EXT4_B2C(sbi, offset + sbi-&gt;s_itb_per_group) &gt;= sb-&gt;s_blocksize)
+		return blk;
