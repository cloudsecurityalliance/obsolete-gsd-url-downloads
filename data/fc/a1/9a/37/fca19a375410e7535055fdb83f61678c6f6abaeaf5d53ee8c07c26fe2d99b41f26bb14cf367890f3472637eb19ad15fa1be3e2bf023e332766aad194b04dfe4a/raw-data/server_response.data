    #5 0x2b9562e in gx_default_transform_pixel_region_process_data ghostpdl/./base/gdevdflt.c:2210:15
    #6 0x2b9562e in gx_default_transform_pixel_region ghostpdl/./base/gdevdflt.c:2240
