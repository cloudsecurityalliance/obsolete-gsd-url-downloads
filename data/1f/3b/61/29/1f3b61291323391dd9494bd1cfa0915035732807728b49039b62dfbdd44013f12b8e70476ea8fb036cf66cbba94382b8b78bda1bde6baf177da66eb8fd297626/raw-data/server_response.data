/*       this array is similar to opcode_type_str_long &amp; etc    */
#define NUM_STATES 5
static const char *state_str[NUM_STATES+1] = {
  &quot;Unknown&quot;,
  &quot;Down/Dead&quot;,
  &quot;Initializing&quot;,
  &quot;Standby&quot;,
  &quot;Ready&quot;,
  &quot;Active/Active-Attention&quot;
};

</pre>
