function afterRenderCollaborationGroupPinnedPost() {
    let pinnedPost = document.getElementsByClassName('forceChatterFeedItem pinned');
    if (pinnedPost.length > 0 && pinnedPost[0].querySelector('a[title="View With Comments"]') == null) {
        let article = pinnedPost[0].querySelector('article');
        let feedElementId = article.getAttribute('data-id');
        let aTag = document.createElement('a');
        aTag.setAttribute('href', '/s/feed/' + feedElementId);
        aTag.setAttribute('title', 'View With Comments');
        aTag.innerText = 'View With Comments';
        article.appendChild(aTag);
    }
}

function afterRenderCollaborationGroupMembers() {
    let groupDetailCard = document.querySelector('.forceChatterGroupDetailCard');
    if (groupDetailCard != null) {
        let groupDetailCardValues = groupDetailCard.querySelectorAll('.slds-form-element__control');
        let memberCount = null;
        groupDetailCardValues.forEach(function(cardValue) {
            if (cardValue.innerText.endsWith(' Members')) {
                memberCount = cardValue.innerText.replace(' Members', '');
            }
        });
        let members = document.querySelector('span[title="Members"]');
        if (members != null && memberCount != null) {
            let membersValue = members.parentElement.children[1];
            let formattedValue = '(' + memberCount + ')';
            membersValue.setAttribute('title', formattedValue);
            membersValue.innerText = formattedValue;
        }
    }
}

function afterRenderCaseViewFull() {
    let formElementLabels = document.querySelectorAll('.slds-form-element__label');
    formElementLabels.forEach(function(formElementLabel) {
        if (formElementLabel.innerText == 'Status') {
            formElementLabel.parentElement.style.display = 'none';
            return;
        }
    });
}

function afterRenderCaseInlineEditFull() {
    let formElementLabels = document.querySelectorAll('.modal-container .slds-form-element');
    formElementLabels.forEach(function(formElementLabel) {
        if (formElementLabel.innerText.includes('Status')) {
            formElementLabel.parentElement.style.display = 'none';
            return;
        }
    });
}

function runAfterAuraInit() {
    if (typeof $A == 'undefined') {
        setTimeout(function(){ runAfterAuraInit(); }, 200);
        return;
    }
    $A.afterRender = (f => function afterRender(){
        consoleLog('$A.afterRender');
        consoleLog(arguments);
        if (arguments.length == 1 && typeof(arguments[0].type) !== 'undefined') {
            let componentType = arguments[0].type;
            consoleLog(componentType);
            if (
              componentType.startsWith('rl-CollaborationGroup-VIEW-RELATED_LIST-') &&
              componentType.search('-GroupMembers-')
            ) {
                afterRenderCollaborationGroupMembers();
            }
        }
        return f.apply(this, arguments);
    })($A.afterRender);

    $A.log = (f => function alog(){
        let alog = arguments[0];
        consoleLog('$A.log: ' + alog);
        if (alog.startsWith('FeedInner.helper#getPageCount')) {
            setTimeout(function(){ afterRenderCollaborationGroupPinnedPost(); }, 1000);
            setTimeout(function(){ afterRenderCollaborationGroupPinnedPost(); }, 2000);
        } else if (alog.includes('record cache HIT for 500') && alog.includes('mode=VIEW')) {
            setTimeout(function(){ afterRenderCaseViewFull(); }, 500);
            setTimeout(function(){ afterRenderCaseViewFull(); }, 1000);
        } else if (alog.includes('record cache HIT for 500') && alog.includes('mode=INLINE_EDIT')) {
            setTimeout(function(){ afterRenderCaseInlineEditFull(); }, 500);
            setTimeout(function(){ afterRenderCaseInlineEditFull(); }, 1000);
        }
        return f.apply(this, arguments);
    })($A.log);
}

