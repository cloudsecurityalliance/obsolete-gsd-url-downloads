n+++ b\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/ParameterValidator.java\n\x40\x40 -74,12 +74,16 \x40\x40 public abstract class ParameterValidator {\n     }\n \n     \/**\n-     * Normalize \x3ccode\x3evalue\x3c\/code\x3e. Returns \x3ccode\x3enull\x3c\/code\x3e if the\n-     * value is considered empty or not present.\n+     * Normalize \x3ccode\x3evalue\x3c\/code\x3e. Removes all backticks unless param.isBacktickAllowed()\n+     * returns true.  Returns \x3ccode\x3enull\x3c\/code\x3e if the value is considered empty\n+     * or not present.\n      * \x40param value the value to normalized\n      * \x40return the normalized value\n      *\/\n     public String normalize(String value) {\n+        if (!param.isBackTickAllowed() && !StringUtils.isBlank(value)) {\n+            value = value.replaceAll(\"`\", \"\");\n+        }\n         return StringUtils.isBlank(value) ? null : value;\n     }\n \ndiff --git a\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/ProbeStateValidator.java b\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/ProbeStateValidator.java\nindex d61117c..e2b8035 100644\n--- a\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/ProbeStateValidator.java\n+++ b\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/ProbeStateValidator.java\n\x40\x40 -46,6 +46,7 \x40\x40 class ProbeStateValidator extends ParameterValidator {\n      * {\x40inheritDoc}\n      *\/\n     public String normalize(String value) {\n+        value = super.normalize(value);\n         if (value != null) {\n             value = value.toUpperCase();\n         }\ndiff --git a\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/test\/CommandTest.java b\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/test\/CommandTest.java\nindex 8765bdd..d70dfef 100644\n--- a\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/test\/CommandTest.java\n+++ b\/java\/code\/src\/com\/redhat\/rhn\/domain\/monitoring\/command\/test\/CommandTest.java\n\x40\x40 -14,6 +14,14 \x40\x40\n  *\/\n package com.redhat.rhn.domain.monitoring.command.test;\n \n+import java.util.HashMap;\n+import java.util.Iterator;\n+import java.util.List;\n+import java.util.Set;\n+\n+import org.apache.commons.collections.Transformer;\n+import org.hibernate.HibernateException;\n+\n import com.redhat.rhn.domain.monitoring.MonitoringConstants;\n import com.redhat.rhn.domain.monitoring.MonitoringFactory;\n import com.redhat.rhn.domain.monitoring.Probe;\n\x40\x40 -23,6 +31,7 \x40\x40 import com.redhat.rhn.domain.monitoring.command.Command;\n import com.redhat.rhn.domain.monitoring.command.CommandGroup;\n import com.redhat.rhn.domain.monitoring.command.CommandParameter;\n import com.redhat.rhn.domain.monitoring.command.Metric;\n+import com.redhat.rhn.domain.monitoring.command.ParameterValidator;\n import com.redhat.rhn.domain.monitoring.command.ThresholdParameter;\n import com.redhat.rhn.domain.monitoring.command.ThresholdType;\n import com.redhat.rhn.domain.monitoring.test.MonitoringFactoryTest;\n\x40\x40 -31,13 +40,6 \x40\x40 import com.redhat.rhn.domain.user.User;\n import com.redhat.rhn.testing.RhnBaseTestCase;\n import com.redhat.rhn.testing.UserTestUtils;\n \n-import org.apache.commons.collections.Transformer;\n-import org.hibernate.HibernateException;\n-\n-import java.util.HashMap;\n-import java.util.Iterator;\n-import java.util.List;\n-\n \/**\n  * CommandTest\n  * \x40version $Rev: 52080 $\n\x40\x40 -193,5 +195,29 \x40\x40 public class CommandTest extends RhnBaseTestCase {\n \n \n     }\n+\n+    public void testBacktickCleansing() throws Exception {\n+        User user = UserTestUtils.findNewUser(\"testUser\", \"testOrg\"  +\n+                        this.getClass().getSimpleName());\n+        Probe probe = MonitoringFactoryTest.createTestProbe(user);\n+        assertNotNull(probe.getCommand().getCommandParameters());\n+        assertTrue(probe.getCommand().getCommandParameters