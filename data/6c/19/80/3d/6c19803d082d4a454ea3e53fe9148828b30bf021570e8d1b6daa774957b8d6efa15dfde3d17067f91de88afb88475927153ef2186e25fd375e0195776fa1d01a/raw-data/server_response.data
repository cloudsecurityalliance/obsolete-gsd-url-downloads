window.Support = {
  searchbox: null,
  subject: "",
  lastKeyPress: null,
  resultsTimer: null,
  empty: true,

  init: function () {
    var me = this,
      timerOn = false;

    me.searchbox = jQuery("#support-search");
    me.searchbox.keyup(function (e) {
      if (e.which === 32 || e.which === 13) {
        Support.findResults(_.escape(me.searchbox.val()), false);
      }
      me.lastKeyPress = new Date().getTime();
      if (timerOn) {
        clearTimeout(me.resultsTimer);
      } else {
        timerOn = true;
      }
      Support.setTimer();
    });
  },

  findResults: function (query, init) {
    query = _.escape(query);
    var me = this;

    me.empty = init || jQuery.trim(query).length === 0;
    if (me.subject !== query || init) {
      if (me.empty) {
        jQuery('#search_results').html('<h3>Featured Answers</h3>');
      } else {
        jQuery("#search_results").html('<div class="loading">Searching...</div>');
      }
      jQuery.ajax({
        url: "/api/support/find_results?query=" + query,
        success: function (data) {
          Support.printResults(data, query);
        }
      });
      me.subject = query;
    }
  },

  printResults: function (data, query) {
    var me = this;

    if (data.length > 0) {
      var list = ['<ul>'];
      jQuery.each(data, function (index, elem) {
        if (elem.subject) {
          var replies = parseInt(elem.post_counter, 10) - 1;
          list.push('<li><a href="/topic/' +
            elem.id + '-' +
            TextHelpers.parameterize(elem.subject) + '">' + elem.subject +
            '</a>' +
            '<div class="excerpt"><a href="/forum/2">Spiceworks Support</a> | ' +
            replies + (replies === 1 ? ' reply' : ' replies') +
            '</div>' +
            '</li>');
        } else if (elem.name) {
          list.push('<li><a href="/help/' + elem.name + '">' + elem.alt_name + '</a>' +
            '<div class="excerpt"><a href="/help">Help Documentation</a></div>' +
            '</li>');
        }
      });
      list.push('</ul>');
      if (me.empty) {
        jQuery('#search_results').append(list.join(' '));
      } else {
        jQuery('#search_results').html(list.join(' '));
      }
    } else {
      jQuery('#search_results').html('<h2>No results found.</h2>');
    }
    if (jQuery.trim(query).length > 0) {
      var info = ['<div class="message-panel message-panel-info">'];
      info.push('<strong>' + (data.length === 0 ? 'Not finding an answer?' : 'Still not finding any answer?') + '</strong>');
      info.push('Submit a question to the support forum.');
      info.push('<div class="ask-question">' +
        '<a href="https://community.spiceworks.com/topic/new?forum_id=2&question=' + query + '" class="sui-bttn">Ask a Question</a>' +
        '</div>');
      info.push('</div>');
      jQuery('#search_results').append(info.join(' '));
    }
  },

  setTimer: function () {
    var me = this;

    me.resultsTimer = setTimeout(function () {
      var currentTime = new Date().getTime();
      if (currentTime - me.lastKeyPress > 1500) {
        Support.findResults(me.searchbox.val());
      }
      Support.setTimer();
    }, 1000);
  }
};

jQuery(document).ready(function () {
	setTimeout(function () {
      Support.init();
      Support.findResults("", true);
    }, 500);
});</script>
