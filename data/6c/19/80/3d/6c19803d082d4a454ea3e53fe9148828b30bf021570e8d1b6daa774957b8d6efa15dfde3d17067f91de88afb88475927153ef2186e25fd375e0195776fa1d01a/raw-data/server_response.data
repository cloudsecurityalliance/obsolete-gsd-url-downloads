  <script type="text/javascript">
    window.zdconsent.cmd.push(function(window) {
      var OneSignal = window.OneSignal || [];

      var browserType = function() {
        if (navigator.userAgent.include('Chrome')) {
          return 1;
        } else if(navigator.userAgent.include('Firefox')) {
          return 2;
        } else {
          console.warn('Browser is not supported for Spiceworks push notifications')
          return 0;
        }
      };

      OneSignal.push(function() {
        OneSignal.init({
          appId: window.oneSignalAppId,
          autoRegister: false,
          notifyButton: { enable: false },
          promptOptions: {
            actionMessage: "We'd like to show you notifications for the latest news and updates.",
            acceptButtonText: "ALLOW",
            cancelButtonText: "NO THANKS"
          },
          subdomainName: window.oneSignalSubDomain || null
        });
      })

      OneSignal.push(function() {
        OneSignal.on('subscriptionChange', function(isSubscribed) {
          if (isSubscribed && window.browserType() > 0) {
            OneSignal.getUserId(function(userId) {
              var loggedIn = false;
              if (loggedIn) {
                jQuery.post('/notifications/push_options', { 'sid' : userId, 'dev_type' : window.browserType });
              }
            });
          }
        });
      });

      /* This check is to detect a broken state that occurs in One Signal when switching between two One Signal apps.
        If a subscribed logged out user logs in and has not allowed push notifications for logged in, they will be in a broken state where the subscription is true but there is no assigned userId. The fix is to reprompt the user. In order to that we have to make One Signal think this user has not been prompted before. This issue with One Signal is current as of 08/07/18 */
      OneSignal.push(function() {
        OneSignal.getUserId(function(userId) {
          if(userId == null) {
            OneSignal.isPushNotificationsEnabled(function(isEnabled) {
              if (isEnabled) {
                if(sessionStorage.getItem("USER_STATUS_FLIPPED") === "true") {
                  sessionStorage.removeItem("USER_STATUS_FLIPPED");
                  localStorage.removeItem("onesignal-notification-prompt");
                  sessionStorage.removeItem("ONESIGNAL_HTTP_PROMPT_SHOWN");
                  OneSignal.showHttpPrompt();
                }
              }
            });
          }
        });
      });

      OneSignal.push(function() {
        OneSignal.on('customPromptClick', function(promptClickResult) {
          GoogleAnalytics.trackEvent('Push Notification Settings', 'click', promptClickResult);
        });
      });
    });
  </script>
  <script type="text/javascript" data-cfasync="false">
    window.zdconsent.cmd.push(function(window) {
      var OneSignal = window.OneSignal || [];

      /* In milliseconds, time to wait before prompting user. This time is relative to right after the user presses <ENTER> on the address bar and navigates to your page */
      var notificationPromptDelay = 300000; // 5 minutes

      var promptAndSubscribeUser = function() {
        OneSignal.isPushNotificationsEnabled(function(isEnabled) {
          if (!isEnabled) {
            OneSignal.showHttpPrompt();
          }
        });
      };

      var promptAfterDelay = function() {
        /* Use navigation timing to find out when the page actually loaded instead of using setTimeout() only which can be delayed by script execution */
        var navigationStart = window.performance.timing.navigationStart;

        /* Get current time */
        var timeNow = Date.now();

        /* Prompt the user if enough time has elapsed */
        setTimeout(promptAndSubscribeUser, Math.max(notificationPromptDelay - (timeNow - navigationStart), 0));
      };

      var promptAfterScroll = function() {
        var ticking = false;

        var scrollListener = function(event) {
          var body    = document.body;
          var html    = document.documentElement;

          /* Get the current scroll Y position */
          var scrollPosition = window.scrollY;

          /* Get the height of the viewport */
          var height = Math.max( body.scrollHeight, body.offsetHeight,
                            html.clientHeight, html.scrollHeight, html.offsetHeight );

          if (!ticking) {

            /* Using 'requesetAnimationFrame' to throttle the function call */
            window.requestAnimationFrame(function() {
              var threshold = height / 2;
              if (scrollPosition > threshold) {
                promptAndSubscribeUser();
              }
              ticking = false;
            });

            ticking = true;
          }
        };

        window.addEventListener('scroll', scrollListener);
      };

      OneSignal.push(promptAfterScroll);
      OneSignal.push(promptAfterDelay);

      OneSignal.push(function () {
        var flag = false;
        OneSignal.on('popoverShown', function () {
          /* GA event showing that the Notification appeared */
          if (flag) { return; }
          GoogleAnalytics.triggerOptimize('onesignal');
          flag = true;
        });
      });
    });
  </script>


