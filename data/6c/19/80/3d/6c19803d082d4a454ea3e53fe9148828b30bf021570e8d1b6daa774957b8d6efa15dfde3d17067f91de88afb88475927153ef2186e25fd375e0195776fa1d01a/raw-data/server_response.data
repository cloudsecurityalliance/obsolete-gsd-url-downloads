  <script type="text/javascript">(function($) {
  $(function() {
    var timeout = 0,
        pathname = window.location.pathname;
    
    if($('#cms-page-controls').is(':visible')) {
      timeout = 500;
    }
    
    
    // anchor code
    //https://jsfiddle.net/ianclark001/aShQL/
      function scroll_if_anchor(href) {
        href = typeof(href) == "string" ? href : jQuery(this).attr("href");
        var fromTop = 50;
        var $target = jQuery(href);

        if($target.selector) {
          console.log('wi if stmt');
          $('html, body').animate({ scrollTop: $target.offset().top - fromTop });
          if(history && 'pushState' in history) {
            history.pushState({}, document.title, window.location.pathname + href);
            return false;
          }
        }
      }    

      // When our page loads, check to see if it contains and anchor
      scroll_if_anchor(window.location.hash);

    // ^anchor code
    
    
    if(pathname === '/support' || pathname === '/support/') {
      // to ensure our JS doesn't conflict with the CMS JS
      setTimeout(function() {
        // hack to use true full-width layout
        $('body').addClass('no-body-shadow');
        $('.support-region').addClass('constrained-content');
        $('.sui-footer').hide();
        $('#community_message ').css('margin-top', '25px');
      }, timeout);
    }
  });
})(window.jQuery);


