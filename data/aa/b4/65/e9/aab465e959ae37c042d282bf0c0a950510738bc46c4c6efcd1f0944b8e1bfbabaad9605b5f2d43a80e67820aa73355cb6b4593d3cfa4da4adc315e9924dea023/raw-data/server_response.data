function getCookie(name) {
    var value = '; ' + document.cookie;
    var parts = value.split('; ' + name + '=');
    if (parts.length === 2) return parts.pop().split(';').shift();
}

window.onload = checkCookieState;
let cookieBackup = [];

function checkCookieState() {
    let now = new Date()
    let inThirtyMinutes = new Date(new Date().setMinutes(now.getMinutes() + 30))
    let inThirteenMonths = new Date(new Date().setMonth(now.getMonth() + 13))
    let cookiesToBackup = [{
        name: '__hssc',
        expiration: inThirtyMinutes
    }, {
        name: '__hssrc',
        expiration: ''  // session
    }, {
        name: '__hstc',
        expiration: inThirteenMonths
    }, {
        name: 'hubspotutk',
        expiration: inThirteenMonths
    }]
    
    function deleteHubspotCookies() {
        cookiesToBackup.forEach(function (cookie) {
            document.cookie = cookie.name + '=; Max-Age=0; domain=.paessler.com; path=/';
        })
    }

    if (getCookie('OptanonAlertBoxClosed')) {
        let consent = getCookie('OptanonConsent');
        if (consent.includes('C0004%3A1')) {
            cookieBackup.forEach(function (cookie) {
                document.cookie =
                    cookie.name + '=' + cookie.value + '; expires=' + cookie.expiration + '; domain=.paessler.com; path=/; SameSite=Lax';
