        } else if (consent.includes('C0004%3A0')){
          	var _hsp = window._hsp = window._hsp || [];
  			_hsp.push(['revokeCookieConsent']);
          	_hsq.push(['doNotTrack']);
            deleteHubspotCookies();
        }
    } else {
        if (cookieBackup.length === 0) {
            let hubspotCookiesAreSet = true;
            cookiesToBackup.forEach(function (cookie) {
                if(!getCookie(cookie.name)){
                    hubspotCookiesAreSet = false;
                }
            })

            if (hubspotCookiesAreSet) {
                cookiesToBackup.forEach(function (cookie) {
                    cookieBackup.push({
                        name: cookie.name,
                        value: getCookie(cookie.name),
                        expiration: cookie.expiration
                    })
                })

                deleteHubspotCookies();
            }
        }

        setTimeout(checkCookieState, 1000)
    }
}
