  addEventListener(&quot;error&quot;, function(e) {
    e.preventDefault();
    postMessage(/* whatever, based on examining e */);
  });
  importScripts(stuff);  // or load a sub-worker and importScripts in it, 
                         // or whatever else we want to test.

