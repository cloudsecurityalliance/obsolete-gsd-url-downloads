/* Content
----------------------------------------------- */
body {font: normal 16px 'Roboto', Arial, sans-serif ;color: #666666;background: #ffffff none repeat scroll top left;padding: 0 0 0 0;}
html body .region-inner {min-width: 0;max-width: 100%;width: auto;}
a:link {text-decoration:none;color: #2a5689;}
a:visited {text-decoration:none;color: #2a5689;}
a:hover {text-decoration:underline;color: #33aaff;}
.body-fauxcolumn-outer .fauxcolumn-inner {background: transparent none repeat scroll top left;_background-image: none;}
.body-fauxcolumn-outer .cap-top {position: absolute;z-index: 1;height: 400px;width: 100%;background: #ffffff none repeat scroll top left;}
.body-fauxcolumn-outer .cap-top .cap-left {width: 100%;background: transparent none repeat-x scroll top left;_background-image: none;}
.content-outer {-moz-box-shadow: 0 0 0 rgba(0, 0, 0, .15);-webkit-box-shadow: 0 0 0 rgba(0, 0, 0, .15);-goog-ms-box-shadow: 0 0 0 #333333;box-shadow: 0 0 0 rgba(0, 0, 0, .15);margin-bottom: 1px;}
.content-inner {padding: 10px 40px;}
.content-inner {background-color: transparent;}
/* Header
----------------------------------------------- */
.header-outer {background: transparent none repeat-x scroll 0 -400px;_background-image: none;}
.Header h1 {font: normal normal 40px 'Roboto', Arial, sans-serif;color: #000000;text-shadow: 0 0 0 rgba(0, 0, 0, .2);}
.Header h1 a {color: #000000;}
.Header .description {font-size: 18px;color: #000000;}
.header-inner .Header .titlewrapper {padding: 22px 0;}
.header-inner .Header .descriptionwrapper {padding: 0 0;}
.header-inner img {width:100%;}
/* Tabs
----------------------------------------------- */
.tabs-inner .section:first-child {border-top: 0 solid #dddddd;}
.tabs-inner .section:first-child ul {margin-top: -1px;border-top: 1px solid #dddddd;border-left: 1px solid #dddddd;border-right: 1px solid #dddddd;}
.tabs-inner .widget ul {background: transparent none repeat-x scroll 0 -800px;_background-image: none;border-bottom: 1px solid #dddddd; margin-top: 0;margin-left: -30px;margin-right: -30px;}
.tabs-inner .widget li a {display: inline-block;padding: .6em 1em;font: normal 16px 'Roboto', Arial, sans-serif ;color: #000000;border-left: 1px solid transparent;border-right: 1px solid #dddddd;}
.tabs-inner .widget li:first-child a {border-left: none;}
.tabs-inner .widget li.selected a, .tabs-inner .widget li a:hover {color: #000000;background-color: #eeeeee;text-decoration: none;}
/* Columns
----------------------------------------------- */
.main-outer {border-top: 0 solid transparent;}
.fauxcolumn-left-outer .fauxcolumn-inner {border-right: 1px solid transparent;}
.fauxcolumn-right-outer .fauxcolumn-inner {border-left: 1px solid transparent;}
/* Headings
----------------------------------------------- */
h2 {margin: 0 0 1em 0;font: normal bold 11px 'Roboto', Arial, sans-serif;color: #000000; text-transform: uppercase;}
/* Widgets
----------------------------------------------- */
.widget .zippy {color: #999999;text-shadow: 2px 2px 1px rgba(0, 0, 0, .1);}
.widget .popular-posts ul {list-style: none;}
/* Posts
----------------------------------------------- */
.date-header span {background-color: #ffffff;color: #666666;padding: 0.4em;letter-spacing: 3px;margin:inherit;position:relative;top:-10px;padding:4px 10px;}
.date-header {border-top:1px dotted #ddd;text-align:center;}
.main-inner {padding-top: 35px;padding-bottom: 65px;}
.main-inner .column-center-inner {padding: 0 0;}
.main-inner .column-center-inner .section {margin: 0 1em;}
.post {margin: 0 0 100px 0;}
h3.post-title, .comments h4 {font: normal normal 22px 'Roboto', Arial, sans-serif;margin: .75em 0 0;}
.post-body {line-height:1.4;position: relative;}
.post-body img, .post-body .tr-caption-container, .Profile img, .Image img,.BlogList .item-thumbnail img {padding:0;background: #ffffff;-moz-box-shadow: 1px 1px 5px rgba(0, 0, 0, .1);-webkit-box-shadow: 1px 1px 5px rgba(0, 0, 0, .1);box-shadow: 1px 1px 5px rgba(0, 0, 0, .1);}
.post-body img{margin:0 0 1em;}
.post-body img, .post-body .tr-caption-container {padding: 0;}
.post-body .tr-caption-container {color: #666666;}
.post-body .tr-caption-container img {padding: 0;background: transparent;border: none;-moz-box-shadow: 0 0 0 rgba(0, 0, 0, .1);-webkit-box-shadow: 0 0 0 rgba(0, 0, 0, .1);box-shadow: 0 0 0 rgba(0, 0, 0, .1);}
.post-body img {width:100%;border:1px solid #aaa;}
.post-body h2{font-size:20px;font-weight:normal;margin:2em 0 1em;color:#666;}
.post-body h3{font-size:16px;font-weight:bold;margin:1em 0 .5em;color:#666;}
.post-body code{word-break:break-all; font-size:0.85em; padding:0.1em 0.2em; color:#333; background-color:#f4f4f4; -moz-border-radius:4px; -webkit-border-radius:4px; -ms-border-radius:4px; -o-border-radius:4px; border-radius:4px;}
.post-body pre{font-size:0.85em; padding:0.5em; color:#333; background-color:#f4f4f4; -moz-border-radius:4px; -webkit-border-radius:4px; -ms-border-radius:4px; -o-border-radius:4px; border-radius:4px}
.post-body pre.wrap {white-space:pre-wrap}
.post-header {margin: 0 0 1.5em;line-height: 1.6;font-size: 90%;}
.post-footer {margin: 20px -2px 0;padding: 5px 10px;color: #666666;background-color: transparent;border-top: 1px solid #eeeeee;line-height: 1.6;font-size: 90%;}
#comments .comment-author {padding-top: 1.5em;border-top: 1px solid transparent;background-position: 0 1.5em;}
#comments .comment-author:first-child {padding-top: 0;border-top: none;}
.avatar-image-container {margin: .2em 0 0;}
#comments .avatar-image-container img {border: 1px solid #eeeeee;}
.jump-link a{padding: 4px 10px;border: 2px solid #2a5689;display: inline-block;border-radius: 4px;color: #2a5689;font-weight: 400;}
/* Comments
----------------------------------------------- */
.comments .comments-content .icon.blog-author {background-repeat: no-repeat;background-image: url(data:image/png;base64,);}
.comments .comments-content .loadmore a {border-top: 1px solid #999999;border-bottom: 1px solid #999999;}
.comments .comment-thread.inline-thread {background-color: transparent;}
.comments .continue {border-top: 2px solid #999999;}
/* Accents
---------------------------------------------- */
.section-columns td.columns-cell {border-left: 1px solid transparent;}
.blog-pager {}
a.button {padding: 4px 10px;border: 2px solid #2a5689;display: inline-block;border-radius: 4px;color: #2a5689;font-weight: 400;text-decoration:none;}
a.button:hover {color: #fff;background:#2a5689;text-decoration:none;}
.feed-links{font-size:2em;text-align:center;padding:40px 0;}
.disabled a.button {border: 2px solid #ccc;color: #ccc;font-weight: 400;}
.disabled a.button:hover{color: #fff;background:#ccc;}
.content.footer{background:#2a5689;}
.content.footer .content-inner{background:transparent;color:#fff;}
.content.footer h2,.content.footer a,.content.footer a:link,.content.footer a:visited,.content.footer p{color:#fff;}
.content.footer a{text-decoration:underline;}
.content.footer h2{font-size:1.2em;font-weight:300;}
input[type="button"]{width:auto !important;}
/* Mobile
----------------------------------------------- */
body.mobile  {background-size: auto;}
.mobile .body-fauxcolumn-outer {background: transparent none repeat scroll top left;}
.mobile .body-fauxcolumn-outer .cap-top {background-size: 100% auto;}
.mobile .content-outer {-webkit-box-shadow: 0 0 3px rgba(0, 0, 0, .15);box-shadow: 0 0 3px rgba(0, 0, 0, .15);}
body.mobile .AdSense {margin: 0 -0;}
.mobile .tabs-inner .widget ul {margin-left: 0;margin-right: 0;}
.mobile .post {margin: 0;}
.mobile .main-inner .column-center-inner .section {margin: 0;}
