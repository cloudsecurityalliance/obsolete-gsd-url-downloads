/* Global Footer */
#footer {padding: 40px 0 20px; color: #000; background: #f2f2f2;font-family: Metric,Arial,Helvetica,sans-serif; font-weight: 400; font-size: 14px; line-height: 1.25;margin-top:20px;}
#footer a:hover {opacity: .5;}
#footer p {color:#4c5255;font-size: 13px;font-family: Metric Regular;}
#footer p a {color:#000;}
#footer p.copyright {color: #909090; padding-bottom: 28px;padding-top:10px;}
#footer p.footer-links {text-align:right;}
#footer p.footer-links a {color:#000; font-weight: 500; position: relative; display: inline-block; padding-right: 8px; padding-left: 8px;font-size:13px;}
#footer div.cppa{text-align: right;}
#footer div.cppa a {font-size:12px;color:#000;}
#footer div.social {margin-bottom: 18px; text-align: right;}
#footer div.social a {display: inline-block;text-decoration: none; padding: 0 20px;}
#footer div.social a:first-child, #footer p.footer-links a:first-child {padding-left: 0;}
#footer div.social a:last-child, #footer p.footer-links a:last-child {padding-right: 0;}


/* Global Styles */
