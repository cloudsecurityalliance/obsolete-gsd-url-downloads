/*Tooltip */
.Tooltip{position:relative;display:inline-block;width:13px;height:13px;margin-top:-2px;margin-left:4px;border-radius:50%;font-size:11px;line-height:1.3;text-align:center;vertical-align:middle;background-color:#8291a1;color:#fff;cursor:help}
.Tooltip:hover .Tooltip-info{opacity:1;visibility:visible;-webkit-transition-delay:0;transition-delay:0;-webkit-transform:translateX(0);transform:translateX(0)}
.Dropdown-tooltip,.Tooltip-info{position:absolute;top:-30px;left:24px;z-index:99999;width:240px;padding:15px 15px 15px 20px;border-radius:2px;font-size:14px;font-weight:400;text-align:left;background-color:#8291a1;opacity:0;visibility:hidden;cursor:text;-webkit-transform:translateX(30px);transform:translateX(30px);-webkit-transition:all .3s ease 0s;transition:all .3s ease 0s;}
.Tooltip--reverse .Dropdown-tooltip,.Tooltip--reverse .Tooltip-info{left:auto;right:24px;-webkit-transform:translateX(-30px);transform:translateX(-30px)}
.Dropdown-tooltip:after,.Tooltip-info:after{content:"";position:absolute;top:30px;left:0;width:0;height:0;margin-left:-14px;border:7px solid transparent;border-right-color:#8291a1}
.Tooltip--reverse .Dropdown-tooltip:after,.Tooltip--reverse .Tooltip-info:after{left:auto;right:0;margin-left:0;margin-right:-14px;border-right-color:transparent;border-left-color:#8291a1}
.Dropdown-tooltip,.Tooltip-info{width:180px;padding:10px}
.Tooltip-normal--reverse .Dropdown-tooltip,.Tooltip-normal--reverse .Tooltip-info{-webkit-transform:translateX(-30px);transform:translateX(-30px);left:auto;right:24px}
.Tooltip-normal--reverse .Dropdown-tooltip:after,.Tooltip-normal--reverse .Tooltip-info:after{left:auto;right:0;margin-left:0;margin-right:-14px;border-right-color:transparent;border-left-color:#8291a1}
.Dropdown-tooltip,.Tooltip-info{width:180px;padding:10px}
.Tooltip-normal--reverse .Dropdown-tooltip,.Tooltip-normal--reverse .Tooltip-info{-webkit-transform:translateX(-30px);transform:translateX(-30px);left:auto;right:24px}
.Tooltip-normal--reverse .Dropdown-tooltip:after,.Tooltip-normal--reverse .Tooltip-info:after{left:auto;right:0;margin-left:0;margin-right:-14px;border-right-color:transparent;border-left-color:#8291a1}


