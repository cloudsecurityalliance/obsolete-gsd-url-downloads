+	var_push_dtor(&amp;var_hash, &amp;pcount);
 
 	--p; /* for ';' */
 	count = Z_LVAL_P(pcount);
@@ -880,7 +882,7 @@ SPL_METHOD(SplObjectStorage, unserialize)
 			if(pelement-&gt;obj) {
 				var_push_dtor(&amp;var_hash, &amp;pelement-&gt;obj);
 			}
