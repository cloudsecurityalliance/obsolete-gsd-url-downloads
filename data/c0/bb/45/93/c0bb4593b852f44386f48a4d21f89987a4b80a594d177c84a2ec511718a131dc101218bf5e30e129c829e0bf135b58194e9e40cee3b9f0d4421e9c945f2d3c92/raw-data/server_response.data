<script>
function openNav(){
	$("#mySidenav").width("280px");
}

function closeNav(){
	$("#mySidenav").width("0px");
	$('#mySidenav').css("margin-right", "0px");
	$(".subnav").width("0px");
	$('.subnav').css("right", "-40px");
}

function closeSubNav(){
	$(".subnav").width("0px");
	$('.subnav').css("right", "-40px");
	$('#mySidenav').css("margin-right", "0px");
	$('.sidenav a').removeClass("active");
}

$(document).ready(function(){
	$('.sidenav a').click(function(){
		$('.sidenav a').removeClass("active");
		$(this).addClass("active");
		$(".subnav").width("0px");
		$('.subnav').css("right", "-40px");
		$('#mySidenav').css("margin-right", "0px");
	});

	$('.dropdown').click(function(){
		var target_id = $(this).attr('data-toggle');
		target_id="#"+target_id;
		$('.dropdown-list').not(target_id).hide();
		$(target_id).slideToggle(function(){
			$(".subnav").width("240px");
			$('.subnav').css("right", "0");
			$('.dropdown-list').css("display", "block");
			$('#mySidenav').css("margin-right", "240px");
			$('.dropdown-list').not(target_id).hide();
		});
	});
});
</script>

