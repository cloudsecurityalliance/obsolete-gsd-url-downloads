<article role="article" about="/node/2062" typeof="schema:Article" class="node node--type-article node--promoted node--view-mode-full node--type-article-full">
  
    <h1 class="title">
      <span property="schema:name">New Stable Releases: Tor 0.3.5.16, 0.4.5.10 and 0.4.6.7</span>

    </h1>
    <span property="schema:name" content="New Stable Releases: Tor 0.3.5.16, 0.4.5.10 and 0.4.6.7" class="hidden"></span>
  <span property="schema:interactionCount" content="UserComments:1" class="hidden"></span>


