                    if (typeof f.now == "function") {
                        d += f.now();
                    }
                }
                u = 'xxxxxxxx-xxxx-4xxx-yxxx-xxxxxxxxxxxx'.replace(/[xy]/g, function(c) {
                    var r = (d + m.random()*16)%16 | 0;
                    d = m.floor(d/16);
