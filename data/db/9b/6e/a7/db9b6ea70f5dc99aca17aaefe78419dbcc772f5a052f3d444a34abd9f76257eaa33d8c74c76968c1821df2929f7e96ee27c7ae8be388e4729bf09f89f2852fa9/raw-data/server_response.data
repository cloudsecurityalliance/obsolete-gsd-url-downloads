  (function($) { 
    $(document).ready(function(){
        function checkDisable(){
            var removeDisable = true;
            $(".information-field").each(function(){
                if($(this).val() == null || $(this).val().trim() == ""){
                    removeDisable = false;
                    return false;
                }
            });
            
            if(removeDisable){
                $("#button-submit").removeClass('lia-button-disabled');
            } else{
                $("#button-submit").addClass('lia-button-disabled');
            }
        }

        $("#login_first_name, #login_last_name").keyup(function(){
            checkDisable();
        });

        $("#user_country").change(function(){
            checkDisable();
        });

        $("#button-submit").click(function(){
            if ( !$(this).hasClass('lia-button-disabled') ) {
                jsonVal = {};
                $(".information-field").each(function(){
                    if($(this).val().trim() != ""){
                        jsonVal [$(this).attr('name')] = $(this).val().trim();
                    }
                });
                jsonVal ['userID'] = '-1';
                $.post('/plugins/custom/alfresco/alfresco/custom.update-user-information',jsonVal,function(data,status){
                    if(status == "success" && data.trim() != 'error'){
                        $(".InfoMessage.lia-panel-feedback-banner-note").slideDown();
                         setTimeout(function() {
                           $(".InfoMessage.lia-panel-feedback-banner-note").slideUp();
                        }, 15000);
                    }
                    if(data.trim() == 'error' || status == "error"){
                        $(".InfoMessage.lia-panel-feedback-banner-alert").slideDown();
                         setTimeout(function() {
                           $(".InfoMessage.lia-panel-feedback-banner-alert").slideUp();
                        }, 15000);
                    }
                    $(".custom-user-information-banner section").hide();
                });
            }
        });

        function setCookie(cname, cvalue, exdays) {
            var d = new Date();
            d.setTime(d.getTime() + (2 * 60 * 1000));
            var expires = "expires="+d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/";
        }
        $("#button-close").click(function(){
            $(".custom-user-information-banner").hide();
            setCookie("show-user-info-banner", "false", 0);
        });
    });     
  })(LITHIUM.jQuery);

