    <article>
      <header>
        <div class="post-title">
          <h1 class="title">CVE-2016-11018</h1>
        </div>
        <div class="post-meta">
          <div class="date">
            <span class="posted-on">
              <i class="fas fa-calendar"></i>
              <time datetime='2016-05-20T00:00:00Z'>
                May 20, 2016
              </time>
            </span>
            <span class="reading-time">
              <i class="fas fa-clock"></i>
