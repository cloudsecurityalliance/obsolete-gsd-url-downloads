// this function allows decimal decoding
var decodeEntities = (function() {
    // this prevents any overhead from creating the object each time
    var element = document.createElement('div');

    function decodeHTMLEntities (str) {
        if(str && typeof str === 'string') {
            // strip script/html tags
            str = str.replace(/<script[^>]*>([\S\s]*?)<\/script>/gmi, '');
            str = str.replace(/<\/?\w(?:[^"'>]|"[^"]*"|'[^']*')*>/gmi, '');
            element.innerHTML = str;
            str = element.textContent;
            element.textContent = '';
        }
        return str;
    }
    return decodeHTMLEntities;
})();

var product="";
var filter="";
var advisoriesDisplayed = [];
var advisoriesFiltered = [];
var currentPage=1;

//this function sets the advisories filtered by product and search
function setAdvisoriesFiltered() {
    advisoriesFiltered = [];
    for (i = 0; i < advisories_list.length ; i++) {
        var name = decodeEntities(advisories_list[i]['name']);
        var id = document.createTextNode(advisories_list[i]['ID']);
        var products = advisories_list[i]['products'];
        var cve = advisories_list[i]['cve'];
        if ((name.toLowerCase().includes(filter.toLowerCase())
        || cve.toLowerCase().includes(filter.toLowerCase())
        || id.nodeValue.toLowerCase().includes(filter.toLowerCase())
        || JSON.stringify(products).toLowerCase().includes(filter.toLowerCase()))
        && JSON.stringify(products).toLowerCase().includes(product.toLowerCase())) {
            advisoriesFiltered.push(advisories_list[i]);
        }
    }
}

//this function sets the advisories that must be displayed
function setAdvisoriesDisplay() {
    advisoriesDisplayed = [];
    if (currentPage > 0) {
        for (i = (currentPage - 1) * 10; i < currentPage * 10 && i < advisoriesFiltered.length; i++) {
            advisoriesDisplayed.push(advisoriesFiltered[i]);
        }
    }
    rebuild_advisories_table(advisoriesDisplayed.reverse());
}

//build pagination bullets
function buildPagination() {
    var nbPages = Math.ceil(advisories_list.length / 10);
    var pagination = document.getElementById("pagination");
    for (i = 1; i <= nbPages; i++) {
        var page = document.createElement('a');
        var pageText = document.createTextNode(i);
        page.appendChild(pageText);
        page.classList.add("page-numbers");
        if (i == currentPage) {
            page.classList.add("current");
        }
        page.onclick = function () {
            setCurrentPage();
	};
        pagination.appendChild(page);
    }
}

//this function sets the paginqtion display
function setPaginationDisplay() {
    var totalPages = Math.ceil(advisories_list.length / 10);
    var nbPages = Math.ceil(advisoriesFiltered.length / 10);
    var pagination = document.getElementById("pagination");

    if (currentPage > nbPages) {
        currentPage = nbPages;
    }
    if (currentPage < 1) {
        currentPage = 1;
    }

    for (i = 1; i <= totalPages; i++) {
        if (i != currentPage && pagination.childNodes[i].classList.contains("current"))
	    currentPage && pagination.childNodes[i].classList.remove("current");
        else if (i == currentPage && !pagination.childNodes[i].classList.contains("current"))
	    currentPage && pagination.childNodes[i].classList.add("current");
        if (i <= nbPages) {
            pagination.childNodes[i].style.display = '';
        }
        else {
            pagination.childNodes[i].style.display = 'none';
        }
    }
}

//this function rebuild the advisories table
function rebuild_advisories_table(advisories) {
    var tableAdv = document.getElementById("tableadv");
    //clean the table
    while (tableAdv.firstChild) {
        tableAdv.removeChild(tableAdv.firstChild);
    }
    var i;
    // create the table lines
    for (i = 0; i < advisories.length; i++) {

	//retrieve the elements
	var name = decodeEntities(advisories[i]['name']);
	var id = document.createTextNode(advisories[i]['ID']);
        var products = advisories[i]['products'];
        // Insert a new line
        var newLine = tableAdv.insertRow(0);

        // ID cell
        var idCell = newLine.insertCell(0);
        var idCellText = id;
        idCell.appendChild(idCellText);

        // Date cell
        var dateCell = newLine.insertCell(1);
        var dateCellText = document.createTextNode(advisories[i]['date'])
        dateCell.appendChild(dateCellText);

        // name cell
        var nameCell = newLine.insertCell(2);
        var link = document.createElement('a');
        var nameCellText = document.createTextNode(name);
        link.appendChild(nameCellText);
        link.title = name;
        link.href = advisories[i]['link'];
        nameCell.appendChild(link);

        // products cell
        var productsCell = newLine.insertCell(3);
        var j;
        for (j = 0; j < products.length; j++) {
            var productLine = document.createElement('span');
	    var cvssVersion = products[j][2];
            productLine.innerHTML = products[j][0] + " ";
            if (cvssVersion == "3.1") {
	        if (products[j][1] < 4)
                    productLine.innerHTML += "<span class=\"lowv3\"> (score " + products[j][1] + ")";
                else if (products[j][1] < 7)
                    productLine.innerHTML += "<span class=\"mediumv3\"> (score " + products[j][1] + ")";
                else if (products[j][1] < 9)
                    productLine.innerHTML += "<span class=\"highv3\"> (score " + products[j][1] + ")";
                else
                    productLine.innerHTML += "<span class=\"criticalv3\"> (score " + products[j][1] + ")";
	    }
	    else {
                if (products[j][1] < 4)
                    productLine.innerHTML += "<span class=\"low\"> (score " + products[j][1] + ")";
                else if (products[j][1] < 7)
                    productLine.innerHTML += "<span class=\"medium\"> (score " + products[j][1] + ")";
                else
                    productLine.innerHTML += "<span class=\"high\"> (score " + products[j][1] + ")";
	    }
            productLine.classList.add("line-product");
            productsCell.appendChild(productLine);
	}
    }
}

function selectProduct() {
    setProduct(document.getElementById("productsList").value);
}

function setProduct(newProduct) {
    product = newProduct;
    setAdvisoriesFiltered();
    setPaginationDisplay();
    setAdvisoriesDisplay();
    if (newProduct === "")
        newProduct = "All Products";
    document.getElementById("productSelected").innerHTML = newProduct;
}

function setFilter(newFilter) {
    filter = newFilter;
    setAdvisoriesFiltered();
    setPaginationDisplay();
    setAdvisoriesDisplay();
}

function setCurrentPage() {
    currentPage = event.target.text;
    setAdvisoriesFiltered();
    setPaginationDisplay();
    setAdvisoriesDisplay();
}

