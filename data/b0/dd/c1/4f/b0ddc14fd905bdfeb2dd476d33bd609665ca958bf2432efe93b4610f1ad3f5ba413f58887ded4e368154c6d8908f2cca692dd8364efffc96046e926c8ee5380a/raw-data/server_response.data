&gt; 
&gt;  if (!defined $Dir) {
&gt;    $Dir = $ENV{&#39;TMPDIR&#39;} || $ENV{&#39;TEMP&#39;} || $ENV{&#39;TMP&#39;} || &quot;/tmp&quot;;
&gt;    $TmpMode = 1;
