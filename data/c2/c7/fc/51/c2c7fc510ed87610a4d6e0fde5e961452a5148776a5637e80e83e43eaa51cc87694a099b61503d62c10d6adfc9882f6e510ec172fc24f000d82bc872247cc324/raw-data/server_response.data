<script type="text/javascript">
	if(!$("body").hasClass("addNewFea")) {
		/* Below Code will Show/Hide Admin view based on User role */
		var viewType = 'null';
		if(viewType != undefined && viewType != null){
			$(".adminActionBtn").removeClass("actselected");
			$(".adminActionBtn[data-viewtype=" + viewType + "]").addClass("actselected");
			if(viewType == "ADMIN"){
				$(".adminNote").show();
			}else {
				$(".adminNote").hide();
			}
		}
		if(user.userAdmin != '' && user.userAdmin != undefined){
	   		if(user.userAdmin == 'true'){
	   			$(".adminAction").show();
	   		}
	   	}
		
		var pagename = '';
		
		/* Below code shows up an info alert message on fresh login by an admin */
		var adminAlertMsg = "You are authorised to view unpublished information by switching to 'Admin View'";
		var adminAlertHTML = "<div><section class='adminAlertMsg'>" + adminAlertMsg + "</section><section class='adminAlertStop'><input type='checkbox' id='adminAlertStopCheckbox'/> Don't show this message again!</section></div>";
		
		var stopAlert = localStorage.getItem('ADMIN_VIEW_ALERT') || '';
		if(user != undefined && user != null && user.id != '' && user.userAdmin == 'true' && stopAlert != 'YES' && pagename != 'Taxonomy'){
			var alerted = 'null' || '';
			if (alerted != 'YES') {
		    	$.alert({
		 			content: adminAlertHTML,
		 			title:'',
		 			useBootstrap: false,
		 			backgroundDismiss: true,
		 			type: 'blue',
		 			buttons: {
		      	         ok: {
		      	             keys: ['enter'],
		      	             action: function () {
		      	                 //do nothing
		      	             }
		      	         }
		      	 	},
		      	 	onClose: function () {
		      	 		$.ajax({
		    				type:"GET",
		    				url:"/feature-explorer/adminViewAlert",
		    				success:function(response){
		    					//do nothing
		    				},
		    				error: sfError
		    			});
		      	 		
		      	 		var stopAdminAlert = $("#adminAlertStopCheckbox").prop("checked");
		      			if(stopAdminAlert == true){
		      				localStorage.setItem('ADMIN_VIEW_ALERT', 'YES');
		      			} else{
		      				//do nothing
		      			}
		      	    }
		 		});
		    }
		} else {
			//do nothing
		}    
	} else {
		//do nothing
	}
	
</script>
