&lt;object data='data:text/html;,&lt;svg version=&quot;1.1&quot; width=&quot;660&quot; height=&quot;342&quot; xmlns=&quot;<a rel="nofollow" href="http://www.w3.org/2000/svg">http://www.w3.org/2000/svg</a>&quot; xmlns:xlink=&quot;<a rel="nofollow" href="http://www.w3.org/1999/xlink">http://www.w3.org/1999/xlink</a>&quot;&gt;&lt;script&gt;console.log(&quot;Remote Image test&quot;)&lt;/script&gt;&lt;image xlink:href=&quot;<a rel="nofollow" href="https://ss0.bdstatic.com/5aV1bjqh_Q23odCf/static/superman/img/logo/bd_logo1_31bdc765.png?x">https://ss0.bdstatic.com/5aV1bjqh_Q23odCf/static/superman/img/logo/bd_logo1_31bdc765.png?x</a>&quot; width=&quot;660&quot; height=&quot;342&quot;/&gt;&lt;/svg&gt;'&gt;&lt;/object&gt;


&lt;!-- CSP works fine --&gt;
&lt;iframe src='data:text/html;,&lt;script&gt;console.log(&quot;Remote Image test&quot;)&lt;/script&gt;&lt;img src=&quot;<a rel="nofollow" href="https://ss0.bdstatic.com/5aV1bjqh_Q23odCf/static/superman/img/logo/bd_logo1_31bdc765.png?x">https://ss0.bdstatic.com/5aV1bjqh_Q23odCf/static/superman/img/logo/bd_logo1_31bdc765.png?x</a>&quot; width=&quot;660&quot; height=&quot;342&quot;/&gt;'&gt;&lt;/iframe&gt;
&lt;/body&gt;
&lt;/html&gt;
===


