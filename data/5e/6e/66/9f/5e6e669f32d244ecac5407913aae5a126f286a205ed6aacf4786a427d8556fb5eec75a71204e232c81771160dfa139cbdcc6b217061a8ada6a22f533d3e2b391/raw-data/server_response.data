	<script type="text/javascript">
		document.addEventListener("DOMContentLoaded", function() {

			document.getElementById("allow_betas").addEventListener("change", function(event) {
				var betaElements = document.querySelectorAll(".beta");
				for (var i = 0; i < betaElements.length; i++) {
					var betaElement = betaElements[i];
					betaElement.style.display = betaElement.style.display === "none" ? "block" : "none";
				}
			});

			if (document.getElementById("allow_betas").checked) {
				var betaElements = document.querySelectorAll(".beta");
				for (var i = 0; i < betaElements.length; i++) {
					var betaElement = betaElements[i];
					betaElement.style.display = "block";
				}
			}

			if (window.location.hash === "#beta") {
				document.getElementById("allow_betas").checked = true;
				var betaElements = document.querySelectorAll(".beta");
				for (var i = 0; i < betaElements.length; i++) {
					var betaElement = betaElements[i];
					betaElement.style.display = "block";
				}
			}

		});
	</script>
