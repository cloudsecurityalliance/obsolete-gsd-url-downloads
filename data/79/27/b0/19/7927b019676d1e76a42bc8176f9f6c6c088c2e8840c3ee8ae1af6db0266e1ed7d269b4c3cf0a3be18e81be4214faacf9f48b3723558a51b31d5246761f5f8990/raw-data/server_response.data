        ; (function ($) {
          $(document).ready(function() {
            // 
            function hideComponent() {
              console.debug('Hiding the On This Page component as no anchors or headings were found');
              $('.rsalink-c-on-this-page-container').hide();
            }
            
            // 
            if ($('.rsalink-u-anchor').length) {
              // 
              $('.rsalink-u-anchor').each(function(){
                let anchorId = $(this).attr('name');
                let anchorText = $(this).text();
                if (anchorText.length) {
                  if ($('.rsalink-c-on-this-page-list__item')[0]) {
                    $('.rsalink-c-on-this-page-list__item').last().after('<li class="rsalink-c-on-this-page-list__item"><a href="#' + anchorId + '">' + anchorText + '</a></li>');  
                  } else {
                    $('.rsalink-c-on-this-page-list').append('<li class="rsalink-c-on-this-page-list__item"><a href="#' + anchorId + '">' + anchorText + '</a></li>');
                  }
                } else {
                  console.debug("Skipping the anchor #" + anchorId + " as its label is empty");
                }
              });
            } else {
              // 
              // 
              let captureQuery = '.lia-message-body-content h1, .lia-message-body-content h2, .lia-message-body-content h3, .lia-message-body-content h4';
              if ($(captureQuery).length) {
                $(captureQuery).each(function(){
                  // 
                  let anchorText = $(this).text();
                  let anchorType = $(this).prop('nodeName');
                  
                  // 
                  if (anchorText.length) {
                    // 
                    let anchorId = $(this).text().replace(/[^a-zA-Z0-9]/g, '');

                    // 
                    $(this).wrapInner('<a id="' + anchorId + '" name="' + anchorId + '" class="rsalink-u-anchor"></a>');

                    // 
                    let anchorClass = 'rsalink-c-on-this-page-list__item  is-' + anchorType.toLowerCase() + '-heading-anchor';
                    if ($('.rsalink-c-on-this-page-list__item')[0]) {
                      $('.rsalink-c-on-this-page-list__item').last().after('<li class="' + anchorClass + '"><a href="#' + anchorId + '">' + anchorText + '</a></li>');  
                    } else {
                      $('.rsalink-c-on-this-page-list').append('<li class="' + anchorClass + '"><a href="#' + anchorId + '">' + anchorText + '</a></li>');
                    }
                  } else {
                    console.debug('Skipping an anchor because there is no label in the standard heading tag');
                  }
                });
              } else {
                hideComponent();
              }
            }
          });
        })(LITHIUM.jQuery);

