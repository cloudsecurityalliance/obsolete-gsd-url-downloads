<script>
	
		const init1 = () => {
			
			// const addBsc = document.getElementById('addBsc');
			const watchButton = document.getElementById('watchToken');
			//Created check function to see if the MetaMask extension is installed
			const isMetaMaskInstalled = () => {
				//Have to check the ethereum binding on the window object to see if it's installed
				const { ethereum } = window;
				return Boolean(ethereum && ethereum.isMetaMask);
			};

			addBsc.addEventListener('click', async () => {
				ethereum.request({
				  method: 'wallet_addEthereumChain',
				  "params": [
					{
					  "chainId": "0x38",
					  "chainName": "Binance Smart Chain (Mainnet)",
					  "rpcUrls": ["https://bsc-dataseed.binance.org"],
					  "nativeCurrency": {
						"name": "BNB",
						"symbol": "BNB",
						"decimals": 18
					  },
					  "blockExplorerUrls": ["https://bscscan.com"]
					}
				  ]
				}).then((success) => {
					if (success) {
					  console.log('Binance Smart Chain (Mainnet) successfully added to wallet network!')
					} else {
					  throw new Error('Something went wrong.')
					}
				  }).catch(console.error);
			});
			
			watchButton.addEventListener('click', async () => {
				
				ethereum.request({
				  method: 'wallet_watchAsset',
				  params: {
					type: 'ERC20',
					options: {
					  address: '0xBdD1c5Ad29153B485Ac54F033C09F7eb935BF0e4',
					  symbol: 'rDAO',
					  decimals: 18,
					  image: 'https://cdn.vnoc.com/assets/TOKEN-RDAO-3.png',
					},
				  },
				}).then((success) => {
					if (success) {
					  console.log('RDAO successfully added to wallet!')
					} else {
					  throw new Error('Something went wrong.')
					}
				  }).catch(console.error);
			});
			
		};
		window.addEventListener('DOMContentLoaded', init1)
		
	</script>
