                },
                error: function() {
                    $('.info-container', divContainer).append('<div class=""><i class="lia-fa fa-var-close" /></div>');
                },
                complete: function() {
                    $('.spinner', divContainer).remove();
                }
            });
        });
        $('body').on('click', '.user-profile-card', function(evt) {
            if (!$(evt.target).hasClass('profile-link')) {
