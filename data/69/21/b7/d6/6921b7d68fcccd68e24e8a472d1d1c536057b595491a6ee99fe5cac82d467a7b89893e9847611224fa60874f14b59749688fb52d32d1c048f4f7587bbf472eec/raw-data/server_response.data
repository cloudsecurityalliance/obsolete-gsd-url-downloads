#!/bin/sh

LOOP=100000
USER=dummy_user
# add
/bin/su -c &quot;exit;&quot; $USER || { /usr/sbin/adduser -m $USER; add=1; }

for ((i=0; i&lt;LOOP; i++))
do
       /bin/su -c &quot;echo '$i' &gt; /dev/null&quot; $USER
done

# del
(( add == 1 )) &amp;&amp; /usr/sbin/userdel -r $USER
exit
