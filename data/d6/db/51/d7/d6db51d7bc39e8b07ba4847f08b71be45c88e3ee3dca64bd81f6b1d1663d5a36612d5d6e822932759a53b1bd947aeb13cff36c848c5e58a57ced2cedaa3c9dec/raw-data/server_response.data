&gt; 325 	#endif
&gt; 326 	while (p &lt; mMilestoneEntries.Elements() + mMilestoneEntries.Length()) {
&gt; 327 	  mozilla::dom::SVGAnimationElement* elem = p-&gt;mTimebase.get();
