 25         {
 26                 $level = $LIS_TECH_SHOWPNAME;
 27         }
 28 }
 29 
 30 $user = new User();
 31 $user-&gt;userId = $user_id;
 32 $user-&gt;username = $username;
 33 $user-&gt;actualName = $fullname;
 34 $user-&gt;email = $email;
 35 $user-&gt;phone = $phone;
 36 $user-&gt;password = $new_pwd;
 37 $user-&gt;level = $level;
 38 $user-&gt;langId = $lang_id;
