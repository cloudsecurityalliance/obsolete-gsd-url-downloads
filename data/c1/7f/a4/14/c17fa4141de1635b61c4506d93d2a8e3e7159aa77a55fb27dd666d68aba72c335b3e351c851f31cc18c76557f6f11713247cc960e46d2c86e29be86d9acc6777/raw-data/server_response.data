::: browser/extensions/pocket/content/main.js
&#64;&#64; +364,5 &#64;&#64;
<span class="quote">&gt;          // Open a new tab with a given url and activate if
&gt;          var _openTabWithUrlMessageId = &quot;openTabWithUrl&quot;;
