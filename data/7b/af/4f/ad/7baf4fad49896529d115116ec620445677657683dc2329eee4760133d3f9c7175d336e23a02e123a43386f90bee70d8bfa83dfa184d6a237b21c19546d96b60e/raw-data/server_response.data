	icon_class_closed: 'icon-section-closed',
	icon_class_opened: 'icon-section-opened',
	
	do_the_toggle: function(element){
	    $( element ).toggleClass(CAPAWIKI_SIDEBAR.icon_class_closed);
	    $( element ).toggleClass(CAPAWIKI_SIDEBAR.icon_class_opened);
	    $( element ).parent().next().toggle();	
	
	},
	
	do_the_spacetree_loop: function(){
	    if ( $('#sidebar_navigation .spacetree .plugin_pagetree_children_loading').length > 0 ){
	    	setTimeout( function() {
				CAPAWIKI_SIDEBAR.do_the_spacetree_loop();
			}, 500);
	    }
	    else {
	    	$('#sidebar_navigation .spacetree').each(function(index, obj) {
	    		if ( $(obj).find('.plugin_pagetree_current').length > 0 ){
	    			CAPAWIKI_SIDEBAR.do_the_toggle($(obj).prev().find('.icon'));
	    		}
	    	})
	    
	 			
	    }
	}
}

$(document).ready(function() {	
	$( "#sidebar_navigation .tabletitle" ).delegate("span.icon","click", function() {
		CAPAWIKI_SIDEBAR.do_the_toggle(this);
	});
	
	$('#sidebar_navigation .tabletitle a').each(function(index, obj) {
            var a_href = $(obj).attr('href');		
    	    if(window.location.href.indexOf( a_href ) > -1) {
    		CAPAWIKI_SIDEBAR.do_the_toggle( $(obj).parent().find('.icon') );
                CAPAWIKI_SIDEBAR.found_correct_space = true;
	    }    	
	})

    if ( CAPAWIKI_SIDEBAR.found_correct_space == false ){
		CAPAWIKI_SIDEBAR.do_the_spacetree_loop();	
	}
// Show pagetreesearch macro   
    setTimeout( function() {
        $("#pagetreesearch").show();
     },10)
});
/* End of custom sidebar */



</script>

