                        $('.v3-contactus .v3-contactus-qrcode, .v3-contactus .v3-contactus-qrcode img').addClass('v3-v3-contactus-qrcode-active');
                        timeCounter = new Date();
                        c_qrcode();
                    })
                    $('div').on('click',function(e) {
                        $('.v3-contactus .v3-contactus-qrcode, .v3-contactus .v3-contactus-qrcode img').removeClass('v3-v3-contactus-qrcode-active');
                        var lab = $('.v3-contactus .hwimg_hover').attr('lab');

                        // 由于组件里的Wechat拼写错了，所以这个地方埋码会出现问题，改正过来
                        if(lab == "WeChart")
                        {
                            lab = "Wechat";
                        }

                        if (new Date() - timeCounter > 3000 && timeCounter != null) {
                            //-----------ga for hover ----
                            if (typeof ga === 'function') {
                                ga('newhuaiweisupport.send', 'event', "contact us" + "+" + sysConfig.countryCode + "+" + sysConfig.language, "QR code", lab);
                                ga('send', 'event', "contact us" + "+" + sysConfig.countryCode + "+" + sysConfig.language, "QR code", lab);
                            }
                            sendDmpaByAttr("",  "contact us",lab,"QR code");
                        }
                        timeCounter = null;
                    })

                } else {
                    $(".v3-contactus a.hwimg_hover").hover(function () {
                        timeCounter = new Date();
                        c_qrcode();
                    }, function () {
                        var cat = "contact us";
                        var act = "QR code";
                        var lab = $(this).attr('lab');

                        // 由于组件里的Wechat拼写错了，所以这个地方埋码会出现问题，改正过来
                        if(lab == "WeChart")
                        {
                            lab = "Wechat";
                        }

                        if (new Date() - timeCounter > 3000) {
                            gaAndAaByAttr(cat,act,lab,"","","");

                            // 3 ha发送
                            var haData = {id:"contact_us_hover_QR_code",title:lab};
                            haDispach(haData);
                            sendDmpaByAttr("",  "contact us",lab,"QR code");
                        }

                        timeCounter = null;
                    })
                }
             //support cookie显示
             function setSupportCookieHeight() {
                let bottomHeight = 0;
                    if($(".huawei-convergent-cookie").length > 0) {
                        if($(".huawei-convergent-cookie").css("display") != "none") {
                            bottomHeight = $(".huawei-convergent-cookie").outerHeight();
                        }
                        $(".support-bootom-cookie").addClass("convergent");
                    } else {
                        bottomHeight = $('.huawei-bootom-cookie').outerHeight();
                    }
                    if(bottomHeight) {
                        $('.support-bootom-cookie').css('bottom',bottomHeight);
                    }
             }

             if(sysConfig.siteCode === "ru_RU" && window.location.pathname.indexOf("support") > -1) {
                let agree = getCookie("agreed-"+sysConfig.countryCode+"-support-cookiepolicy");
                if(!agree) {
                    setTimeout(function () {
                        setSupportCookieHeight();
                        $('.support-bootom-cookie').show();
                     },100)
                }
             }
             
             $(".support-bootom-cookie .support-cookie-cnt .support-cookie-close").on("click", function () {
                setCookie("agreed-"+sysConfig.countryCode+"-support-cookiepolicy", true);
                $('.support-bootom-cookie').remove();
            });
            
            $(window).resize(function() {
                setSupportCookieHeight();
            });

        })
