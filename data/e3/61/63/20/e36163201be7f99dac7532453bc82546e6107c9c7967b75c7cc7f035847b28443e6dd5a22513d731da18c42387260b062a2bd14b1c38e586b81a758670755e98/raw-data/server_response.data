<script type="text/javascript">
	window.dataLayer = window.dataLayer || [];

	function gtmObjectClone(obj) {
	    var objClone;
	    if (obj.constructor == Object) {
	        objClone = new obj.constructor();
	    } else {
	        objClone = new obj.constructor(obj.valueOf());
	    }
	    for (var key in obj) {
	        if (objClone[key] != obj[key]) {
	            if (typeof(obj[key]) == 'object') {
	                objClone[key] = gtmObjectClone(obj[key]);
	            } else {
	                objClone[key] = obj[key];
	            }
	        }
	    }
	    objClone.toString = obj.toString;
	    objClone.valueOf = obj.valueOf;
	    return objClone;
	}

	//设置AT公共变量
	function addATCommonFields(obj){
		var newObj = gtmObjectClone(obj);
		newObj.level1 = "611994";
		newObj.level2 = "1";
		newObj.chapter1 = getATChapter1();

		var chapter2 = getATChapter2();
		var chapter3 = getATChapter3();
		if(chapter2){
			newObj.chapter2 = chapter2;
		}
		if(chapter3){
			newObj.chapter3 = chapter3;
		}
		newObj.atPageName = getATPageName();
		newObj.userCategory = "";
		return newObj;
	}

	function getATChapter1(){
		var pageType = window.digitalData.page.category.pageType;
		pageType = pageType.split("_")[0];
		var chapter1 = "";
		switch(pageType){
		 case "homepage":
			 chapter1 = "Home";break;
		 case "support":
			 chapter1 = "support";break;
		 case "product-detail":
			 chapter1 = "Mkt products";break;
		 case "product-listing":
			 chapter1 = "Mkt products";break;
		  default:
			  chapter1 = window.digitalData.page.category.primaryCategory;
		}
		return chapter1;
	}

	function getATChapter2(){
		var pageType = window.digitalData.page.category.pageType;
		pageType = pageType.split("_")[0];
		var atChapter2 = window.digitalData.page.category.subCategory1;
		if(pageType == "product-detail" || pageType == "product-listing"){
			atChapter2 = window.digitalData.page.category.primaryCategory;
		}
		return atChapter2;
	}

	function getATChapter3(){
		var atChapter3 = window.digitalData.page.category.subCategory2;
		var pageType = window.digitalData.page.category.pageType;
		pageType = pageType.split("_")[0];
		if(pageType == "product-detail"){
			atChapter3 =  window.digitalData.page.category.subCategory1;
		}
		return atChapter3;
	}

	function getATPageName(){
		var pageName = "";
		var pathArr = location.pathname.split("/");
		for(var index = pathArr.length - 1; index > 0; index --){
			if(pathArr[index] != ""){
				pageName = pathArr[index];
				if(pageName.indexOf(".html") != -1){
					pageName = pageName.split(".")[0];
				}
				break;
			}
		}
		return pageName;
	}

	/* *
	 *  判断是否APP内嵌，设置siteId
	 * */
	function getMktDmpaSiteId(){
		var dmpa_siteId = "";
		if(window.integrationJsInterface ||window.integrationJsInterfaceWebview){
			dmpa_siteId = "HuaweiStore-app";
		}else{
			dmpa_siteId = $(window).width() < 1199.98?"HuaweiStore-wap":"HuaweiStore-web";
		}
		return dmpa_siteId;
	}

	var DMPA_SCRIPT_BASEURL = "";
	var dmpa_init_mkt_dmpa_params = {
		"siteId": getMktDmpaSiteId(),
		"IP":null ,
		"userId": null,
		"sessionId": null ,
		"country" : digitalData.page.pageInfo?digitalData.page.pageInfo.siteCode2:"",
		"siteLanguage": digitalData.page.pageInfo.language?digitalData.page.pageInfo.language.replace("_","-"):"",
